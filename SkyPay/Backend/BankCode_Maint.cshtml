<script>
    var Table;

    $(document).ready(function () {
        pageLoad();
    });

    function pageLoad() {
           if ('@CheckLoginPermission' == 'false') {
                location.href = "Login.cshtml";
            }

           if ('@CheckPermission' == 'false') {
            parent.errorAlertCallback("权限不足", function () {
                parent.GoPage("Home");
                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
            });
        }

        AdminObject = parent.AdminObject;
        api = parent.api;

        createBankCodeTable();
        getBankCodeTableResult();

        $('#table_BankCodeTable tbody').on('click', 'tr', function () {
            if ($(this).hasClass('selected')) {
                $(this).removeClass('selected');
            } else {
                Table.$('tr.selected').removeClass('selected');
                $(this).addClass('selected');
            }
        });
    };

    function createBankCodeTable() {
        Table = $("#table_BankCodeTable").DataTable({
            data: [],
            columns: [
                { "title": "银行代码", "data": "BankCode" },
                {
                    "title": "状态", "data": "BankState",
                    "render": function (data) {
                        if (data == 0) {
                            return '<span style="color:blue;">启用</span>';
                        } else {
                            return '<span style="color:red;">停用</span>';
                        }
                    }
                },
                { "title": "银行名称", "data": "BankName" },
                {
                    "title": "银行类型", "data": "BankType",
                    "render": function (data) {
                        if (data == 0) {
                            return '一般银行';
                        } else if (data == 1) {
                            return 'ETH区块链货币';
                        }
                    }
                },
                { "title": "ETH 合约代码(区块链货币)", "data": "ETHContractNumber" }
            ],
            rowCallback: function (row, data) { },
            filter: true,
            info: false,
            ordering: true,
            processing: true,
            retrieve: true,
            selected: true,
            //rowReorder: {
            //    selector: 'td:nth-child(2)'
            //},
            scrollX: true,
            autoWidth: false,
            language: parent.chineseTableLang
        });
    }

    function getBankCodeTableResult() {
        api.getBankCodeTableResult(function (success, obj) {
            Table.clear().draw();
            if (obj.ResultCode == 0) {
                Table.rows.add(obj.BankCodeResults).draw(); // Add new data
                parent.wrapperFadeOut();
            } else {
                var message = ""
                switch (obj.ResultCode) {
                    case 5:
                        message = "没有资料";
                        parent.warningAlert(message);
                        break;
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    default:
                        message = "其他错误";
                        parent.warningAlert(message);
                        break;
                }
                parent.wrapperFadeOut();


            }
        });
    }

    $('#btnadd').click(function () {
        var data = {
            backID: window.frameElement.id.split('_')[1]
        }

        var tmpdata = {
            type: "BankCode_Maint",
            data: data
        };
        sessionStorage.setItem("TempData", JSON.stringify(tmpdata));
        parent.GoPage("BankCode_Add", "银行新增", "BankCode_Add.cshtml");

    });

    $('#btndisable').click(function () {
        if (Table.row(".selected").data() == undefined) {
            parent.warningAlert("请先选择要修改的资料");
        } else {
            swal({
                title: "是否要停用？",
                type: "warning",
                showCancelButton: true,
                closeOnConfirm: true,
                confirmButtonText: "确定",
                cancelButtonText: "取消",
                confirmButtonColor: "#DD6B55",
            },
                function (isConfirm) {
                    if (isConfirm) {

                        disableBankCode(Table.row(".selected").data().BankCode);
                    }
                });
        }

    })

    function disableBankCode(bankCode, currencyType) {
        api.disableBankCode(bankCode, function (success, obj) {
            if (obj.ResultCode == 0) {
                parent.refreshPage(window.frameElement.id.split('_')[1]);
            } else {
                switch (obj.ResultCode) {
                    case 4:
                        parent.errorAlertCallback("权限不足", function () {
                            parent.ChangePage("Home");
                            parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                        });
                        break;

                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;

                    default:
                        parent.warningAlert("其他错误");
                        break;
                }
            }
        });
    }


    //編輯
    $("#btnedit").on("click", function () {

        if (Table.row(".selected").data() != undefined) {

            var backID = window.frameElement.id.split('_')[1];
            parent.CreateSessionStorage("BankCode_Maint", Table.row(".selected").data(), `银行修改(${Table.row(".selected").data().BankName})`, "BankCode_Update.cshtml", backID, "BankCode_Update_" + Table.row(".selected").data().BankCode);

        } else {
            parent.warningAlert("请先选择要修改的资料");
        }

    })
</script>