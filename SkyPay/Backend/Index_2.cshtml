    function createDoughnutChart(data) {
        var percentAmounts = [];
        var backgroundColors = [];
        var labels = [];
        var percenthtml = '';
        var array_li = [];

        if (data.length > 0) {
            getSummaryCompanyByDateTableResult(data[0].ServiceType);
        }

        $('#DoughnutChart_percentages').empty();
        for (var i = 0; i < data.length; i++) {
            var color = createColor();
            percentAmounts.push(data[i].SummaryNetAmounts);
            backgroundColors.push(color);
            labels.push(data[i].ServiceTypeName);

            array_li.push('<li class="col-3" style="border-top: 4px solid ' + color + '" ><p class= "os text-muted" >' + data[i].ServiceTypeName + '</p ><p class="os-percentage">' + data[i].SummaryNetAmountPercent + '<sup>%</sup></p></li >');
        }
        if (array_li.length != 0) {
            if (array_li.length <= 4) {
                var doc_ul = $('<ul class="os-percentages row"></ul>');
                for (var i = 0; i < array_li.length; i++) {
                    doc_ul.append(array_li[i]);
                }
                $('#DoughnutChart_percentages').append(doc_ul);
            } else {

                for (var i = 0; i < array_li.length; i++) {
                    if (i == 0) {
                        doc_ul = $('<ul class="os-percentages row"></ul>');
                    }

                    doc_ul.append(array_li[i]);

                    if (i / 4 == 0 && i != 0) {
                        $('#DoughnutChart_percentages').append(doc_ul);
                        doc_ul = $('<ul class="os-percentages row"></ul>');
                    }
                }

                if (doc_ul.children().length > 0) {
                    $('#DoughnutChart_percentages').append(doc_ul);
                }
            }
        }

        var ctx = document.getElementById('canvas_DoughnutChart').getContext('2d');

        //修正切換資料有殘影的問題
        if (DoughnutChart) {
            DoughnutChart.destroy();
        }

        DoughnutChart = new Chart(ctx, {
            type: 'doughnut',
            data: {
                labels: labels,
                datasets: [{
                    backgroundColor: backgroundColors,
                    data: percentAmounts
                }]
            },
            options: {
                scrollX: true,
                legend: {
                    position: 'top',
                },
                title: {
                    display: false,
                    text: ''
                },
                animation: {
                    animateScale: true,
                    animateRotate: true
                },
                onClick: function (doc, e) {

                    if (e.length > 0) {
                        var serviceTypeEle = data.find(function (element) {
                            return element.ServiceTypeName == e[0]._view.label
                        })
                        getSummaryCompanyByDateTableResult(serviceTypeEle.ServiceType);
                    }
                }
            }
        });
        DoughnutChart.update();

    }

