    function createPointHistoryLineChartData(data) {

        // y 軸的顯示
        var yAxis = [];
        // 資料集合，之後只要更新這個就好了。
        var datas = [];
        //設定Y軸數據
        switch (seldays) {
            case "1":
                for (var i = 1; i < 25; i++) {
                    yAxis.push(i + "时");
                }
                break;
            case "7":
                var StartDate = new Date();
                StartDate.setDate(StartDate.getDate() - 7);
                for (var i = 0; i < 7; i++) {
                    yAxis.push(dateFormate(StartDate.setDate(StartDate.getDate() + 1)));
                }
                break;
            case "15":
                var StartDate = new Date();
                StartDate.setDate(StartDate.getDate() - 15);
                for (var i = 0; i < 15; i++) {
                    yAxis.push(dateFormate(StartDate.setDate(StartDate.getDate() + 1)));
                }
                break;
            case "30":
                var StartDate = new Date();
                StartDate.setDate(StartDate.getDate() - 30);
                for (var i = 0; i < 30; i++) {
                    yAxis.push(dateFormate(StartDate.setDate(StartDate.getDate() + 1)));
                }
                break;
            default:
                break;
        }
        var tmp_index = 0;
        for (var i = 0; i < yAxis.length; i++) {
            var tmpdata = "";
            if (seldays == "1") {
                if (tmp_index < data.length && yAxis[i] == data[tmp_index].CreateDate2 + "时") {
                    tmpdata = data[tmp_index].Value;
                    datas.push(tmpdata);
                    tmp_index++;
                }
            }
            else {
                if (tmp_index < data.length && yAxis[i] == data[tmp_index].CreateDate2) {
                    tmpdata = data[tmp_index].Value;
                    datas.push(tmpdata);
                    tmp_index++;
                }
            }


            if (tmpdata == "") {
                datas.push(0);
            }
        }

        if (seldays == "30") {
            var tmp_yAxis = [];
            for (var i = 0; i < yAxis.length; i++) {
                if (i % 3 == 0 || i == yAxis.length - 1) {
                    tmp_yAxis.push(yAxis[i]);
                }
            }
            yAxis = tmp_yAxis;

            var tmp_data = [];
            tmp_data.push(datas[0]);
            for (var i = 0; i < datas.length; i++) {
                if (i != 0 && i % 3 == 0) {
                    tmp_data.push(datas[i] + datas[i - 1] + datas[i - 2]);
                }
            }

            tmp_data.push(datas[datas.length - 1]);

            datas = tmp_data;
        }


        var ctx = document.getElementById('canvasLine').getContext('2d');

        //修正切換資料有殘影的問題
        if (lineChart) {
            lineChart.destroy();
        }

        lineChart = new Chart(ctx, {
            type: 'line',
            data: {
                labels: yAxis,
                datasets: [{
                    label: '钱包金额',
                    data: datas,
                    backgroundColor: "rgba(0,148,255,0.6)",
                    lineTension: 0
                }]
            },
            options: {
                legend: {
                    display: false
                },
                tooltips: {
                    mode: 'label',
                    label: 'mylabel',
                    callbacks: {
                        label: function (tooltipItem, data) {
                            return tooltipItem.yLabel.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                        }
                    }
                },
                scales: {
                    yAxes: [{
                        ticks: {

                            callback: function (value, index, values) {
                                return value.toLocaleString();
                            }
                        }
                    }]
                }
            }

        });

        lineChart.update();
    }

