    //#region 商戶錢包資料
    function reloadCompanyPoint() {

        $("#divCompanyPoint").empty();
        let strCompanyPointDetail = "";

        api.getCompanyPointTableResult(selCompanyid, function (s, o) {
            if (s) {
                if (o.ResultCode == 0) {
                    tempCompanyPoint = o.CompanyPoints;
                    $.each(o.CompanyPoints, function (i, item) {
                        if (selCurrencyType == o.CompanyPoints[i].CurrencyType) {
                            var canUsePoint = toCurrency(o.CompanyPoints[i].PointValue - o.CompanyPoints[i].LockPointValue - o.CompanyPoints[i].FrozenPoint);
                            strCompanyPointDetail += '<strong style="font-size:18px;">帐户金额： ' + toCurrency(o.CompanyPoints[i].PointValue) + ' ,可用金额 : ' + canUsePoint + ' ,不可用金额 : ' + toCurrency(o.CompanyPoints[i].LockPointValue) + ' ,冻结金额 : ' + toCurrency(o.CompanyPoints[i].FrozenPoint) + '</strong>';
                        }
                    })

                    $("#divCompanyPoint").html(strCompanyPointDetail);
                } else {
                    switch (o.ResultCode) {
                        case 4:
                            parent.errorAlertCallback("权限不足", function () {
                                parent.ChangePage("Home");
                                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                            });
                            break;
                        case 5:
                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        default:
                            parent.warningAlert("其他错误");
                            break;
                    }
                }
            }
        });
    }
    //#endregion

    function dateFormate(date) {
        var today = new Date(date);
        var year = today.getFullYear().toString();
        var month = (today.getMonth() + 1) < 10 ? "0" + (today.getMonth() + 1).toString() : (today.getMonth() + 1).toString();
        var date = today.getDate() < 10 ? "0" + (today.getDate().toString()).toString() : today.getDate().toString();

        return year + "/" + month + "/" + date;

    }

    function changeCurrency() {
        //餘額
        reloadCompanyPoint();
        getDoughnutChartData();
        getCompanyPointHistoryTableResult();
    }

    function createColor() {
        var max = 255;
        var min = 125;
        var r = Math.floor(Math.random() * (max - min) + min);
        var g = Math.floor(Math.random() * (max - min) + min);
        var b = Math.floor(Math.random() * (max - min) + min);
        var color = 'rgba(' + r + ',' + g + ',' + b + ',0.8)';
        return color;
    }
    //切換日期事件
    $('a[data-toggle="tab"]').on('shown.bs.tab', function () {
        seldays = $(this).attr("data-day") // activated tab;
        getDoughnutChartData();
        getCompanyPointHistoryTableResult();
    });

</script>
