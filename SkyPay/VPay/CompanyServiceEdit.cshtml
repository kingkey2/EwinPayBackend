    function changeNotifyLabelText(serviceType) {
       
        if (checkServiceTypeData) {
            if (serviceType == 0) {     
                $('#notifyCollectRate').text("");
                $('#notifyMinOnceAmount').text("");
                $('#notifyMaxOnceAmount').text("");

            } else {
                var _serviceType = serviceType;
                var _currencyType = 'CNY';

                var selectedServiceTypeIndex = checkServiceTypeData.findIndex(function (d) { return d.ServiceType == _serviceType && d.CurrencyType == _currencyType })
                if (selectedServiceTypeIndex == -1) {
                    parent.warningAlert("渠道选择错误");
                    return false;
                } else {
                    selectedServiceType = checkServiceTypeData[selectedServiceTypeIndex];

                    if (selectedServiceType.isUpLine == 0) {
                        $('#notifyCollectRate').text("(需超过或等于" + selectedServiceType.CollectRate + "%" + ")");
                        $('#notifyMinOnceAmount').text("(需超过或等于" + toCurrency(selectedServiceType.MinOnceAmount) + ")");
                        $('#notifyMaxOnceAmount').text("(最高不可超过" + toCurrency(selectedServiceType.MaxOnceAmount) + ")");
                    } else {
                        $('#notifyCollectRate').text('');
                        $('#notifyMinOnceAmount').text('');
                        $('#notifyMaxOnceAmount').text('');
                    }
                }


            }

            return true;
        }

    }

