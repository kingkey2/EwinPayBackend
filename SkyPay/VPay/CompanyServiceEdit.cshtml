                        if (obj.Results[i].isSelected == 1) {
                            $('#pre-selected-options').append("<option value='" + obj.Results[i].CompanyID + "' selected=true>" + obj.Results[i].CompanyName + "</option>");
                        } else {
                            $('#pre-selected-options').append("<option value='" + obj.Results[i].CompanyID + "'>" + obj.Results[i].CompanyName + "</option>");
                        }     
                    }
             
                    $('#pre-selected-options').multiSelect('refresh');
                }
                else {

                    switch (obj.ResultCode) {
                        case 4:
                            parent.errorAlertCallback("权限不足", function () {
                                parent.ChangePage("Home");
                                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                            });
                            break;
                        case 5:

                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        default:
                            parent.warningAlert("其他错误");
                            break;
                    }
                    parent.wrapperFadeOut();
                }
            }
           
        });

    }

</script>
