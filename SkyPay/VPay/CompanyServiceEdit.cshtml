    $(document).ready(function () {
        if ('@CheckLoginPermission' == 'false') {
                location.href = "Login.cshtml";
            }

           if ('@CheckPermission' == 'false') {
            parent.errorAlertCallback("权限不足", function () {
                parent.GoPage("Home");
                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
            });
        }

        pageLoad();

        $('#pre-selected-options').multiSelect({
            selectableHeader: "<input type='text' style='width:300px;height:40px;margin-bottom:10px;' class='search-input form-control' autocomplete='off' placeholder='查询'>",
            selectionHeader: "<input type='text' style='width:300px;height:40px;margin-bottom:10px;' class='search-input form-control' autocomplete='off' placeholder='查询'>",
            afterInit: function (ms) {
                var that = this,
                    $selectableSearch = that.$selectableUl.prev(),
                    $selectionSearch = that.$selectionUl.prev(),
                    selectableSearchString = '#' + that.$container.attr('id') + ' .ms-elem-selectable:not(.ms-selected)',
                    selectionSearchString = '#' + that.$container.attr('id') + ' .ms-elem-selection.ms-selected';

                that.qs1 = $selectableSearch.quicksearch(selectableSearchString)
                    .on('keydown', function (e) {
                        if (e.which === 40) {
                            that.$selectableUl.focus();
                            return false;
                        }
                    });

                that.qs2 = $selectionSearch.quicksearch(selectionSearchString)
                    .on('keydown', function (e) {
                        if (e.which == 40) {
                            that.$selectionUl.focus();
                            return false;
                        }
                    });
            },
            afterSelect: function (value) {
                this.qs1.cache();
                this.qs2.cache();
                if (value.length !=1) {
                    parent.warningAlert("系统有误");
                } else {
                    setCompanyServiceRelationByEditView(value[0], true);
                }
             
            },
            afterDeselect: function (value) {
                this.qs1.cache();
                this.qs2.cache();
                if (value.length != 1) {
                    parent.warningAlert("系统有误");
                } else {
                    setCompanyServiceRelationByEditView(value[0], false);
                }
            }
        });
    });
    function pageLoad() {
        AdminObject = parent.AdminObject;
        api = parent.api;

        createCompanySelect();
        createCompanyServiceTable();
        getServiceTypeByEditProviderService();
        createSetCompanyServiceModal();
        new AutoNumeric.multiple('.thousand-symbols', 'floatPos', { allowDecimalPadding: false, decimalPlaces: 4, modifyValueOnWheel: false });
        new AutoNumeric.multiple('.percent', 'integer', { allowDecimalPadding: false, decimalPlaces: 4, minimumValue: 0, maximumValue: 100 });

        parent.wrapperFadeOut();
    }

    function createSetCompanyServiceModal() {
        $('#SetCompanyServiceModalContent').html("");
        var dochtml = ' <div class="modal fade" style="background-color: rgba(0, 0, 0, 0.4);" data-backdrop=false id="SetCompanyServiceModal" tabindex="-1" role="dialog" aria-hidden="true">' +
            '<div class="modal-dialog" role="document">' +
            '<div class="modal-content">' +
            '<div class="modal-header">' +
            ' <h4 class="title">商户通道调整</h4>' +
            '</div>' +
            '<div class="modal-body">' +
            '<span style="display:block;margin-bottom: 5px;">商戶:<span id="modal_CompanyName" style="margin-left:5px"></span></span>' +
            '<span style="display:block;margin-bottom: 5px;">费率:<input data-rule="percent" style="margin-left:5px" class="percent" id="input_CollectRate" type="text"><label id="notifyCollectRate" style="color:red;margin-bottom:5px;margin-left:5px;"></label></span>' +
            '<span style="display:block;margin-bottom: 5px;">最低金额:<input data-rule="currency" style="margin-left:5px" class="thousand-symbols" id="input_MinOnceAmount" type="text"><label id="notifyMinOnceAmount" style="color:red;margin-bottom: 5px;margin-left:5px;"></label></span>' +
            '<span style="display:block;margin-bottom: 5px;">最高金额:<input data-rule="currency" style="margin-left:5px" class="thousand-symbols" id="input_MaxOnceAmount" type="text"><label id="notifyMaxOnceAmount" style="color:red;margin-bottom: 5px;margin-left:5px;"></label></span>' +
            '<span style="display:block;margin-bottom: 5px;">每日用量:<input data-rule="currency" style="margin-left:5px" class="thousand-symbols" id="input_MaxDaliyAmount" type="text"></span>' +  
            '<input id="input_CurrencyType" style="display:none" />' +
            '<input id="input_ServiceType" style="display:none" />'+
            '<span style="display:block">状态:' +
            '<div class="radio inlineblock"><input type="radio" name="State" id="State0" class="with-gap" value="0"><label for="State0">启用</label></div>' +
            '<div class="radio inlineblock"><input type="radio" name="State" id="State1" class="with-gap" value="1"><label for="State1">停用</label></div>' +
            '</span>' +
            '<span style="display:block">结算时间:' +
            '<div class="radio inlineblock"><input type="radio" name="CheckoutType" id="CheckoutType0" class="with-gap" value="0"><label for="CheckoutType0">当日结算</label></div>' +
            '<div class="radio inlineblock"><input type="radio" name="CheckoutType" id="CheckoutType1" class="with-gap" value="1"><label for="CheckoutType1">隔日结算</label></div>' +
            '<div class="radio inlineblock"><input type="radio" name="CheckoutType" id="CheckoutType2" class="with-gap" value="2"><label for="CheckoutType2">当周结算</label></div>' +
            '</span>' +
            '</div>' +
            '<div class="modal-footer">' +
            '<button id="saveCompanyServiceBtn" onclick="saveCompanyService()" type="button" class="btn btn-primary btn-round waves-effect">确认</button>' +
            '<button onclick="contentcancel()" type="button" class="btn btn-primary btn-round waves-effect">取消</button>' +
            '</div>' +
            '</div>' +
            '</div>' +
            '</div>';
        $('#SetCompanyServiceModalContent').append(dochtml);
   
    }

    function setCompanyServiceRelationByEditView(companyID, isAddRelation) {
        var serviceType = $("#select_ServiceTypeByEditProviderService").val();
        var providerCode = $("#select_Provider").val();
        var currencyType = 'CNY';
        if (serviceType == "0") {
            parent.warningAlert("尚未选择支付通道");
            return;
        }

        if (providerCode == "0") {
            parent.warningAlert("尚未选择供应商")
            return;
        }
        parent.wrapperFadeIn(); 
        api.setCompanyServiceRelationByEditView(companyID, serviceType, currencyType, providerCode, isAddRelation, function (success, obj) {
            if (success) {
                parent.wrapperFadeOut();
                if (obj.ResultCode == 0) {
                    if ($('#select_Company').val() == companyID) {
                        getCompanyServiceTableResult(companyID);
                    }
                    parent.successAlert();
                }
                else {
                    getCompanyServiceRelationByEditView(providerCode, serviceType);
            
                    var message = ""
                    switch (obj.ResultCode) {
                        case 4:
                            parent.errorAlertCallback("权限不足", function () {
                                parent.ChangePage("Home");
                                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                            });
                            break;
                        case 6:
                            parent.warningAlert(obj.Message);
                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        default:
                            parent.warningAlert("其他错误");
                            break;
                    }
                }
            }
        });
    }

    function saveCompanyService() {

        if (!selectedServiceType) {
            parent.warningAlert("渠道选择错误");
            return;
        }

        if ($("#input_CollectRate").val().trim() == "") {
            parent.warningAlert("尚未输入费率(%)");
            return;
        }

        if (!Number.isInteger(parseInt($("#input_CollectRate").val().trim())) || $("#input_CollectRate").val().trim() > 100 || $("#input_CollectRate").val().trim() < 0) {
            parent.warningAlert("费率(%)只能介于0~100");
            return;
        }

        if ($("#input_MinOnceAmount").val().trim() == "") {
            parent.warningAlert("尚未输入最低金额");
            return;
        }

        if ($("#input_MaxOnceAmount").val().trim() == "") {
            parent.warningAlert("尚未输入最高金额");
            return;
        }

        if ($("#input_MaxDaliyAmount").val().trim() == "") {
            parent.warningAlert("尚未输入交易最大用量");
            return;
        }

        if (toNumber($("#input_MinOnceAmount").val()) > toNumber($("#input_MaxOnceAmount").val())) {
            parent.warningAlert("最低金额不可超过最高金额");
            return;
        }

        //目前新增的商戶為其他商戶下線,則驗證是否設定的費率大於上線的交易費率
        if (selectedServiceType.isUpLine == 0) {
            if (selectedServiceType.CollectRate > $("#input_CollectRate").val().trim()) {
                parent.warningAlert("费率(%) 需超过或等于上线额");
                return;
            }


            if (toNumber($("#input_MinOnceAmount").val()) < selectedServiceType.MinOnceAmount) {
                parent.warningAlert("最低金额 需超过或等于上线额");
                $("#input_MinOnceAmount").focus();
                return;
            }

            if (toNumber($("#input_MaxOnceAmount").val()) > selectedServiceType.MaxOnceAmount) {
                parent.warningAlert("最高金额 不可超过上线额");
                return;
            }

        }

        var companyID = $('#select_Company').val();
        var collectRate = $("#input_CollectRate").val().trim();
        var maxDaliyAmount = $("#input_MaxDaliyAmount").val().trim();
        var minOnceAmount = $("#input_MinOnceAmount").val().trim();
        var maxOnceAmount = $("#input_MaxOnceAmount").val().trim();
        var state = $('input[name="State"]:checked').val();
        var checkoutType = $('input[name="CheckoutType"]:checked').val();

        $('#saveCompanyServiceBtn').attr("disabled", true);
        
        if (CompanyServiceType == 'add') {

            var serviceType = $("#select_ServiceType").val().split('_')[0];
            var currencyType = $("#select_ServiceType").val().split('_')[1];

            api.insertCompanyServiceByEditView(companyID, serviceType, currencyType, collectRate, maxDaliyAmount, minOnceAmount, maxOnceAmount, state, checkoutType, function (success, obj) {
                if (success) {
                    $('#saveCompanyServiceBtn').attr("disabled", false);
                    if (obj.ResultCode == 0) {
                        parent.successAlert(function () {
                            getCompanyServiceTableResult(companyID);
                            $('#SetCompanyServiceModal').modal('hide');
                        });
                    }
                    else {
                        var message = ""
                        switch (obj.ResultCode) {
                            case 4:
                                parent.errorAlertCallback("权限不足", function () {
                                    parent.ChangePage("Home");
                                    parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                });
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            case 8:
                                parent.warningAlert("渠道资料已存在");
                                break;
                            case 10:
                                parent.warningAlert("上线额度不足");
                                break;
                            default:
                                parent.warningAlert("其他错误");
                                break;
                        }
                    }
                }
            });
        } else if (CompanyServiceType == 'edit') {
            var serviceType = $("#input_ServiceType").val();
            var currencyType = $("#input_CurrencyType").val();

            api.getGPayRelationByCompany(companyID, serviceType, currencyType, function (success, obj) {
                if (success) {
   
                    if (obj.ResultCode == 0) {
                        var providerMinAmount = 0;
                        var providerMaxAmount = 0;
                        var providerCostRate = 0;
                        var providerMaxDaliyAmount = 0;
                        for (var i = 0; i < obj.ProviderServices.length; i++) {
                            if (i == 0) {
                                providerCostRate = obj.ProviderServices[i].CostRate;
                                providerMinAmount = obj.ProviderServices[i].MinOnceAmount;
                                providerMaxAmount = obj.ProviderServices[i].MaxOnceAmount;
                                providerMaxDaliyAmount = obj.ProviderServices[i].MaxDaliyAmount;
                            } else {
                                if (providerCostRate < obj.ProviderServices[i].CostRate) {
                                    providerCostRate = obj.ProviderServices[i].CostRate;
                                }

                                if (providerMinAmount > obj.ProviderServices[i].MinOnceAmount) {
                                    providerMinAmount = obj.ProviderServices[i].MinOnceAmount;
                                }
                                if (providerMaxAmount < obj.ProviderServices[i].MaxOnceAmount) {
                                    providerMaxAmount = obj.ProviderServices[i].MaxOnceAmount;
                                }
                                if (providerMaxDaliyAmount > obj.ProviderServices[i].MaxDaliyAmount) {
                                    providerMaxDaliyAmount = obj.ProviderServices[i].MaxDaliyAmount;
                                }
                            }
                        }

                        var showText = "";
                        if (collectRate < providerCostRate) {
                            showText += `供应商费率: ${providerCostRate},目前设定费率:${collectRate}<br/>`;
                        }

                        if (minOnceAmount < providerMinAmount) {
                            showText += `供应商最小值: ${providerMinAmount},目前设定最小值:${minOnceAmount}<br/>`;
                        }

                        if (maxOnceAmount > providerMaxAmount) {
                            showText += `供应商最大值: ${providerMaxAmount},目前设定最大值:${maxOnceAmount}<br/>`;
                        }
                        if (showText != "") {
                            swal({
                                title: "当前设定超出供应商额度,是否确认更新?",
                                type: "warning",
                                text: showText,
                                html: true,
                                showCancelButton: true,
                                closeOnConfirm: false,
                                confirmButtonText: "确定",
                                cancelButtonText: "取消",
                                confirmButtonColor: "#DD6B55",
                            },
                                function (isConfirm) {
                                    if (isConfirm) {
                                        updateCompanyServiceByEditView(companyID, serviceType, currencyType, collectRate, maxDaliyAmount, minOnceAmount, maxOnceAmount, state, checkoutType);
                                    } else {
                                        $('#saveCompanyServiceBtn').attr("disabled", false);
                                    }
                                });
                        } else {
                            updateCompanyServiceByEditView(companyID, serviceType, currencyType, collectRate, maxDaliyAmount, minOnceAmount, maxOnceAmount, state, checkoutType);
                        }


                    } else {
                        var message = ""
                        switch (obj.ResultCode) {
                            case 4:
                                parent.errorAlertCallback("权限不足", function () {
                                    parent.ChangePage("Home");
                                    parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                });
                                break;
                            case 5:
                                updateCompanyServiceByEditView(companyID, serviceType, currencyType, collectRate, maxDaliyAmount, minOnceAmount, maxOnceAmount, state, checkoutType);
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            default:
                                parent.warningAlert("其他错误");
                                $('#saveCompanyServiceBtn').attr("disabled", false);
                                break;
                        }
                    }
                }
            
            });
   

        }
    }

    function updateCompanyServiceByEditView(companyID, serviceType, currencyType, collectRate, maxDaliyAmount, minOnceAmount, maxOnceAmount, state, checkoutType) {
        api.updateCompanyServiceByEditView(companyID, serviceType, currencyType, collectRate, maxDaliyAmount, minOnceAmount, maxOnceAmount, state, checkoutType, function (success, obj) {
            if (success) {
                $('#saveCompanyServiceBtn').attr("disabled", false);
                if (obj.ResultCode == 0) {
                    parent.successAlert(function () {
                        getCompanyServiceTableResult(companyID);
                        $('#SetCompanyServiceModal').modal('hide');
                    });
                }
                else {
                    var message = ""
                    switch (obj.ResultCode) {
