        api.getCurrency(function (success, obj) {
            if (success) {
                $("#selectCurrencyType").empty();
                if (obj.ResultCode == 0) {
                    var datas = obj.Currencies;
                    $.each(datas, function (i, v) {
                        if (i == 0) {
                            $("#selectCurrencyType").append('<option data-value="' + v.CurrencyType + '">' + v.CurrencyType + '</option>');
                        } else {
                            $("#selectCurrencyType").append('<option data-value="' + v.CurrencyType + '">' + v.CurrencyType + '</option>');
                        }
                    })
                } else {
                    switch (obj.ResultCode) {
                        case 4:
                            parent.errorAlertCallback("权限不足", function () {
                                parent.ChangePage("Home");
                                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                            });
                            break;
                        case 5:
                            parent.warningAlert("没有资料");
                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        default:
                            parent.warningAlert("其他错误");
                            break;
                    }
                }
                $('#selectCurrencyType').selectpicker('refresh');
            }
        });

        if (AdminObject.CompanyType == 0) {
            $('#div_CompanyType0').show();
        }

        getServiceType();
        //取得商戶資料並啟用下拉選單
        api.getCompanyTableResult(function (success, obj) {
            if (success) {
                if (obj.ResultCode == 0) {
                    //根據取得的資料重做商戶的彈出視窗
                    updateSortable(obj.CompanyResults, "CompanyName", "CompanyID", "ParentCompanyID");
                    if (AdminObject.CompanyType == 2) {
                        $("#companyLabel").attr("data-id", AdminObject.forCompanyID);
                        $("#companyLabel").text(AdminObject.CompanyName);
                    }

                    parent.wrapperFadeOut();
                }
            }
        });
        //儲存
        $("#btn_CompanyCreate_Save").on("click", function () {
            //#region 驗證
            var serviceType = "";
            if ($("#CompanyCode").val() == "") {
                parent.warningAlert("商户代码为必填");
                return;
            }

            if (!($("#CompanyCode").val().trim().indexOf(" ") == -1)) {
                parent.warningAlert("商户代码不能有空格");
                return;
            }

            if ($("#selectCurrencyType").val() == "") {
                parent.warningAlert("尚未選擇幣別");
                return;
            }

            

            //#endregion
            var companyName = $("#CompanyCode").val().trim();
            var companyCode = $("#CompanyCode").val().trim();
            var URL = $('#URL').val().trim();
            var companyType = $('input[name=CompanyType]:checked').val();
            var currencyType = $("#selectCurrencyType").val();
            var withdrawType = $('input[name=WithdrawType]:checked').val();
            var backendLoginIPType = $('input[name=BackendLoginIPType]:checked').val();
            if (withdrawType == "1") {
                if ($("#select_ServiceType").val() == 0) {
                    parent.warningAlert("API代付支付方式不得为空");
                    return;
                }
            }
            serviceType = $("#select_ServiceType").val();

            var parentCompanyID = $('#companyLabel').attr('data-id');
            if (parentCompanyID == undefined && AdminObject.CompanyType != 0) {
                parent.warningAlert("尚未选择上线商户");
                return;
            }
            var contacterName = $("#ContacterName").val().trim();
            var contacterMobile = "";
            var contacterMethod = 0;
            var contacterMethodAccount = $("#ContacterMethodAccount").val().trim();
            var contacterEmail = "";

            var withdrawAPIType = 0
            if ($("#WithdrawAPIType1:checked").val()) {
                withdrawAPIType += parseInt($("#WithdrawAPIType1:checked").val());
            }
            if ($("#WithdrawAPIType2:checked").val()) {
                withdrawAPIType += parseInt($("#WithdrawAPIType2:checked").val());
            }

            var backendWithdrawType = $('input[name=BackendWithdrawType]:checked').val();
            var providerGroupID ="0";

            api.insertCompanyTableResult(companyName, companyCode, URL, companyType, parentCompanyID, contacterName, contacterMobile, contacterMethod, contacterMethodAccount, contacterEmail, withdrawType, serviceType, backendLoginIPType, withdrawAPIType, backendWithdrawType, providerGroupID, currencyType, function (success, obj) {
                if (success) {
                    if (obj.ResultCode == 0) {
                        TempData = {
                            "type": "afterModifyCompany",
                            "data": obj.CompanyResult.CompanyID
                        }
                        sessionStorage.setItem("TempData", JSON.stringify(TempData));
                        var showMessage = "<span style='color:blue'>商户密钥</span></br>" + obj.CompanyResult.CompanyKey + "</br><span style='color:red'>密钥只会显示这一次,请先存取至文档内</span>" 
                        parent.successAlert4(showMessage,function () {
                            parent.refreshPage(EditData.data.backID);
                            parent.CloseActivePage(window.frameElement.name);
                        });
                    } else {
                        switch (obj.ResultCode) {
                            case 4:
                                parent.errorAlertCallback("权限不足", function () {
                                    parent.ChangePage("Home");
                                    parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                });
                                break;
                            case 5:
                                parent.warningAlert("没有资料");
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            case 8:
                                parent.warningAlert("商户代码已存在");
                                break;
                            default:
                                parent.warningAlert("其他错误");
                                break;
                        }
                    }
                } else {

                }
            });
        })
        //取消回檢視頁面
        $("#btn_CompanyCreate_Cancel").on("click", function () {
            parent.ChangePage(EditData.data.backID);
            parent.CloseActivePage(window.frameElement.name);
        })
        //選擇商戶
        $('#btn_selectCompany').click(function () {
            parent.showSelectCompany_menu($('#companyLabel'), window.frameElement.id);
        });
    }

