    function checkedWithdrawAPIType(providerAPIType) {
        // 0=無/1=代收/2=代付/4=查詢餘額/8=查詢單/16=補單
        var b_ProviderAPITypeStr = "";
        var numProviderAPIType = parseInt(providerAPIType);
        if (numProviderAPIType != 0) {
            if ((numProviderAPIType & 1) == 1) {
                b_ProviderAPITypeStr += "后台申请,"
            }
            if ((numProviderAPIType & 2) == 2) {
                b_ProviderAPITypeStr += "API代付,"
            }
     
            if (b_ProviderAPITypeStr.length > 0) {
                b_ProviderAPITypeStr = b_ProviderAPITypeStr.substring(0, b_ProviderAPITypeStr.length - 1);
            }
        }

        return b_ProviderAPITypeStr;
    }

