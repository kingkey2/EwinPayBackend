<script>
        var CompanyServiceTable;
        var WithdrawLimitTable;
        var CompanyWithdrawRelationTable;
        var CData;
        var CSeleData;
        var seleCompanyid;
        var selectedParentCompanyID;
        var boolCreatedCompanyServiceTable = false;
        var isCreatedWithdrawLimitTable = false;
        var isCreatedCompanyWithdrawRelationTable = false;
        var isCreatedTable = false;
        var selectCurrencyType;
        var tempCompanyPoint = null;
        var CompanyServicePointTable;

        $(document).ready(function () {
            if ('@CheckPermission' == 'false') {
            parent.errorAlertCallback("权限不足", function () {
                parent.GoPage("Home");
                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
            });
        }

            pageLoad();
        });
    function pageLoad() {
        if ('@CheckLoginPermission' == 'false') {
                location.href = "Login.cshtml";
            }

            AdminObject = parent.AdminObject;
            api = parent.api;

            if (AdminObject.CompanyType == 1) {
                $('.hideDiv').hide();
            }
  
            createCompanyServicePointTable();
            createCompanyTree();
            //ShowAllCompanyPoint();

            //新增商戶
            $("#btnCompanyCreate").on("click", function () {
                var data = {
                    backID: window.frameElement.id.split('_')[1]
                }

                var tmpdata = {
                    type: "Company_Create",
                    data: data
                };
                sessionStorage.setItem("TempData", JSON.stringify(tmpdata));
                parent.GoPage("Company_Create", "商户新增", "Company_Create.cshtml");

            });
            //編輯商戶
            $("#btnCompanyEdit").on("click", function () {
                if ($('#select_company').val() == 0) {

                    parent.warningAlert("尚未选择商户");
                } else {
                    CSeleData = jQuery.grep(CData, function (n, i) {
                        return (n.CompanyID == seleCompanyid);
                    });
                    var backID = window.frameElement.id.split('_')[1];
                    parent.CreateSessionStorage("Company_Maint", CSeleData[0], `商户编辑(${CSeleData[0].CompanyName})`, "Company_Update.cshtml", backID, "Company_Update_" + CSeleData[0].CompanyName.CompanyCode);
                }
            });
            //停用
            $("#btnCompanyDisable").on("click", function () {
    
                if ($('#select_company').val() == 0) {
                    swal({
                        title: "请选择一间商户",
                        type: "warning",
                        confirmButtonText: "确认",
                        closeOnConfirm: true
                    });
                } else {
                    swal({
                        title: "是否要停用?",
                        type: "warning",
                        showCancelButton: true,
                        closeOnConfirm: false,
                        confirmButtonText: "确定",
                        cancelButtonText: "取消",
                        confirmButtonColor: "#DD6B55",
                    },
                        function (isConfirm) {
                            if (isConfirm) {
                                api.disableCompanyByID(seleCompanyid, function (success, obj) {
                                    if (success) {
                                        if (obj.ResultCode == 0) {
                                            swal({
                                                title: "该商户已停用！",
                                                type: "success",
                                                confirmButtonText: "确认",
                                                closeOnConfirm: true
                                            }, function () {
                                                TempData = {
                                                    "type": "afterModifyCompany",
                                                    "data": seleCompanyid
                                                }

                                                sessionStorage.setItem("TempData", JSON.stringify(TempData));
                                                parent.refreshPage(window.frameElement.id.split('_')[1]);
                                            })
                                        } else {
                                            switch (obj.ResultCode) {
                                                case 4:
                                                    parent.errorAlertCallback("权限不足", function () {
                                                        parent.ChangePage("Home");
                                                        parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                                    });
                                                    break;
                                                case 5:
                                                    // warningAlert("沒有資料");
                                                    break;
                                                case 7:
                                                    parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                                    break;
                                                default:
                                                    parent.warningAlert("其他错误");
                                                    break;
                                            }
                                        }
                                    } else {
                                    }
                                });
                            }
                        });
                }
            });

            //新增商戶渠道資料
            $('#btnCompanyServiceAdd').on("click", function () {

                var data = {
                    CompanyID: seleCompanyid,
                    ParentCompanyID: selectedParentCompanyID,
                    backID: window.frameElement.id.split('_')[1]
                }

                var tmpdata = {
                    type: "CompanyServiceAdd",
                    data: data
                };
                sessionStorage.setItem("TempData", JSON.stringify(tmpdata));
                parent.GoPage("CompanyServiceAdd", `商户渠道新增(${CSeleData[0].CompanyName})`, "CompanyService_Add.cshtml");

            })

            //編輯商戶渠道資料
            $("#btnCompanyServiceEdit").on("click", function () {
                if (CompanyServiceTable.row(".selected").data() != undefined) {
                    CompanyServiceTable.row(".selected").data().forCompanyID = seleCompanyid;
                    CompanyServiceTable.row(".selected").data().ParentCompanyID = selectedParentCompanyID;

                    var backID = window.frameElement.id.split('_')[1];
                    parent.CreateSessionStorage("CompanyService_Maint", CompanyServiceTable.row(".selected").data(), `商户渠道修改(${CSeleData[0].CompanyName})`, "CompanyService_Update.cshtml", backID, "CompanyService_Update_" + CompanyServiceTable.row(".selected").data().forCompanyID + "_" + CompanyServiceTable.row(".selected").data().ServiceType);
                } else {
                    parent.warningAlert("请先选择要修改的资料");
                }
            })

            //停用商戶渠道資料
            $('#btnCompanyServiceDisable').on("click", function () {
                if (CompanyServiceTable.row(".selected").data() == undefined) {
                    parent.warningAlert("请先选择要修改的资料");
                } else {
                    swal({
                        title: "是否要(停用/启用)？",
                        type: "warning",
                        showCancelButton: true,
                        closeOnConfirm: true,
                        confirmButtonText: "确定",
                        cancelButtonText: "取消",
                        confirmButtonColor: "#DD6B55",
                    }, function (isConfirm) {
                        if (isConfirm) {
                            disableCompanyService(seleCompanyid, CompanyServiceTable.row(".selected").data().ServiceType, CompanyServiceTable.row(".selected").data().CurrencyType);
                        }
                    });
                }
            })
            //檢視設定的商戶
            $("#btnShowGPayRelation").on("click", function () {

                var targetData = CompanyServiceTable.row(".selected").data();

                
                if (CompanyServiceTable.row(".selected").data() != undefined) {  
                    var companyServiceData = CompanyServiceTable.row(".selected").data();
                    api.getGPayRelationResult(companyServiceData.forCompanyID, companyServiceData.ServiceType, companyServiceData.CurrencyType, function (s, obj) {
                        if (obj.ResultCode == 0) {
                            var strswaltext = "";
                            strswaltext += '<div class="row clearfix justify-content-center">';
                            $.each(obj.GPayRelations, function (i, item) {
                                strswaltext += '<div class="col-lg-6 col-md-6 col-sm-6">' +
                                    '<div class="card" style="border:1px solid #49c5b6" >' +
                                    '<div class="header">' +
                                    '<strong style="color:#49c5b6;font-size:18px">' + obj.GPayRelations[i].ProviderName + '</strong>' +
                                    '</div> ' + '</div ></div >';
                            })
                            strswaltext += '</div> ';
                            swal({
                                title: targetData.ServiceTypeName,
                                text: strswaltext,
                                html: true,
                                confirmButtonText: "确认"
                            });
                          
                        } else {
                            var message = ""
                            switch (obj.ResultCode) {
                                case 4:
                                    parent.errorAlertCallback("权限不足", function () {
                                        parent.ChangePage("Home");
                                        parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                    });
                                    break;
                                case 7:
                                    parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                    break;
                                case 5:
                                    parent.warningAlert("尚未设定供应商渠道");
                                    break;
                                default:
                                    break;
                            }
                        }
                    });
                } else {
                    parent.warningAlert("请先选择渠道资料");
                }
            })
            //檢視設定的商戶(代付)
            $("#btnShowGPayWithdrawRelation").on("click", function () {

                var targetData = CompanyWithdrawRelationTable.row(".selected").data();

                if (CompanyWithdrawRelationTable.row(".selected").data() != undefined) {

                    api.getGPayWithdrawRelationByCompanyID(seleCompanyid, function (success, obj) {
                        if (obj.ResultCode == 0) {
