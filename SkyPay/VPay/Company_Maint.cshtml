        function disableCompanyService(companyID, serviceType, currencyType) {
            api.disableCompanyService(companyID, serviceType, currencyType, function (success, obj) {
                if (obj.ResultCode == 0) {

                    TempData = {
                        "type": "afterModifyCompany",
                        "data": companyID
                    }

                    sessionStorage.setItem("TempData", JSON.stringify(TempData));
                    parent.successAlert(function () {
                        parent.refreshPage(window.frameElement.id.split('_')[1]);
                    });
                } else {
                    switch (obj.ResultCode) {
                        case 4:
                            parent.errorAlertCallback("权限不足", function () {
                                parent.ChangePage("Home");
                                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                            });
                            break;
                        case 14:
                            parent.warningAlert("验证码有误");
                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        default:
                            parent.warningAlert("其他错误");
                            break;
                    }
                }
            });
        }

