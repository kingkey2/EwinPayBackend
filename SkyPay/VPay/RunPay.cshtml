    function getProxyProviderGroup(providerCode) {
        parent.wrapperFadeIn();
        api.getProxyProviderGroupTableResultByAdmin(providerCode, function (success, obj) {
            if (success) {
                parent.wrapperFadeOut();
                $('#ProxyGroupTable>tbody').empty();
             
                if (obj.ResultCode == 0) {
                    proxyProviderGroupDataState0 = obj.Results.filter(o => o.State == 0);
                    proxyProviderGroupData = obj.Results;
                    for (var i = 0; i < proxyProviderGroupData.length; i++) {
                        if (i == 0) {
                            if (!istmpTrTaCreate) {
                               
                                var tmpTrTa = $('<tr></tr>');
                                tmpTrTa.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td class="TrTd" id="TrTd_${proxyProviderGroupData[i].GroupName}" style="font-size:15px;color:red">0</td >`)

                            }

                            var tmpTr = $('<tr></tr>');
                            tmpTr.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td style="font-size:15px;color:red">${toCurrency(proxyProviderGroupData[i].CanUsePoint)}</td >`)
                        } else if (i % 3 == 0) {

                            if (!istmpTrTaCreate) {
                                $('#ProxyGroupTableForTotalAmount>tbody').append(tmpTrTa);
                                tmpTrTa = $('<tr></tr>');
                                tmpTrTa.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td class="TrTd" id="TrTd_${proxyProviderGroupData[i].GroupName}" style="font-size:15px;color:red">0</td >`)
                            }
                            $('#ProxyGroupTable>tbody').append(tmpTr);
                            tmpTr = $('<tr></tr>');
                            tmpTr.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td style="font-size:15px;color:red">${toCurrency(proxyProviderGroupData[i].CanUsePoint)}</td >`)
                        } else {
                            if (!istmpTrTaCreate) {
                                tmpTrTa.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td class="TrTd" id="TrTd_${proxyProviderGroupData[i].GroupName}" style="font-size:15px;color:red">0</td >`)
                            }
                            tmpTr.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td style="font-size:15px;color:red">${toCurrency(proxyProviderGroupData[i].CanUsePoint)}</td >`)
                        }

                        if (i == proxyProviderGroupData.length - 1) {
                            $('#ProxyGroupTable>tbody').append(tmpTr);
                            if (!istmpTrTaCreate) {
                                $('#ProxyGroupTableForTotalAmount>tbody').append(tmpTrTa);
                            }
                        }
                    }
                    istmpTrTaCreate = true;
                }
                else {
                    var message = ""
                    switch (obj.ResultCode) {
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        case 5:

