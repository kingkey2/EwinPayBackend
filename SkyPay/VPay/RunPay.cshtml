    function confirmManualPayment(objbtn, status) {
        var rowdata = $(objbtn).parent().parent();
        var paymentSerial = rowdata.find('td').eq(0).html();
        var providerCode = rowdata.find('td').eq(2).find('.select_providerCode').val();
        var providerGroup = rowdata.find('td').eq(2).find('.select_providerGroup').val();
        var serviceType = rowdata.find('td').eq(3).find('.select_serviceType').val();
        if (providerCode == "-1") {
            parent.warningAlert("尚未选择充值渠道");
            return;
        }

        if (serviceType == "-1") {
            parent.warningAlert("尚未选择充值类型");
            return;
        }
        if (rowdata.find('td').eq(2).find('.select_providerCode option:selected').data("providertype") ==  "1"){
            if (providerGroup == "-1") {
                parent.warningAlert("尚未选择群组");
                return;
            }
        }
      
        var status = status;
        api.confirmManualPaymentForRunPay(paymentSerial, status, providerCode, serviceType, providerGroup, function (success, obj) {
            if (success) {
             
                parent.wrapperFadeOut();
                if (obj.ResultCode == 0) {
                    modifyRowValueConfirm(paymentSerial, obj.PatmentResult.PaymentData);
                    swal({
                        title: "审核完成",
                        type: "success",
                        confirmButtonText: "确认",
                        closeOnConfirm: true
                    });

                }
                else {
                    switch (obj.ResultCode) {
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        case 4:
                            parent.errorAlert("没有权限", "Login.cshtml");
                            break;
                        case 6:
                            parent.warningAlert(obj.PatmentResult.Message);
                            break;
                        default:
                            message = "其他错误";
                            parent.warningAlert(message);
                            break;
                    }
                }
            } else {
                parent.warningAlert("网路异常");
                parent.wrapperFadeOut();
            }

        });
    }

    function getPaymentTableResult() {
        var companyid = $('#companyLabel').attr("data-id");
        var dateinputval = $('#dateinput').val().split('-');
        var startDate = dateinputval[0];
        var endDate = dateinputval[1];
        var orderID = $('#input_OrderID').val().trim();
        var processStatus = $('#select_processStatus').val().split(",");
        var providerCode = $('#select_ProviderCode').val();
        var serviceType= $('#select_ServiceType').val();

        api.getPaymentReportReviewResult(companyid, startDate, endDate, orderID, processStatus, providerCode, serviceType, function (success, obj) {
            if (isCreatedTable) {
                Table.clear().draw();
            }
            $('.TrTd').text('0');
            if (obj.ResultCode == 0) {
                if (!isCreatedTable) {
                    isCreatedTable = true;
                    //建立資料顯示Table

                    $("#div_table_PaymentTable").empty();
                    $("#div_table_PaymentTable").append('<table style="width:100%" class="table table-bordered table-striped table-hover dataTable nowrap" id="table_PaymentTable"></table>');
                    createPaymentTable(obj.PaymentTableResults);

                } else {
                    Table.rows.add(obj.PaymentTableResults).draw(); // Add new data
                }

                var paymentData = obj.PaymentTableResults;
                var totalAmountByGroupID = [];
                paymentData.reduce(function (res, value) {
                    if (value.GroupName && value.ProcessStatus==4) {
                        if (!res[value.GroupName]) {
                            res[value.GroupName] = { Id: value.GroupName, Amount: 0 };
                            totalAmountByGroupID.push(res[value.GroupName])
                        }
                        res[value.GroupName].Amount += value.OrderAmount;
                    }
                
                    return res;
                }, {});

                for (var i = 0; i < totalAmountByGroupID.length; i++) {
                    $('#TrTd_' + totalAmountByGroupID[i].Id).text(toCurrency(totalAmountByGroupID[i].Amount));
                }
             
                Table.columns.adjust();
            }
            else {
          
                var message = ""
                switch (obj.ResultCode) {
                    case 5:
                        message = "没有资料";
                        parent.warningAlert(message);
                        break;
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    default:
                        message = "其他错误";
                        parent.warningAlert(message);
                        break;
                }

            }
        });
    }

