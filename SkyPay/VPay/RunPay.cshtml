    function createProcessStatus() {
        //0=新建(準備 PaymentSerial)/1=尚未提交/2=交易成功，尚未通知商戶/3=交易失敗/4=交易成功，通知商戶成功/5=供應商資料不一致/6=系統問題
        $("#select_processStatus").append('<option value="99">' + "全部" + '</option>');
        $("#select_processStatus").append('<option value="1">' + "未处理" + '</option>');
        $("#select_processStatus").append('<option value="3">' + "失败" + '</option>');
        $("#select_processStatus").append('<option value="4">' + "成功" + '</option>');
        $("#select_processStatus").append('<option value="6">' + "系统问题" + '</option>');
        $("#select_processStatus").append('<option value="8">' + "上游审核中" + '</option>');
        $('#select_processStatus').selectpicker('refresh');
    }



