                        //withdrawalCreate(serviceType, serviceTypeName, amount)
                        return '<button data-servicetype="' + data + '" data-servicetype="' + rowdata.ServiceType + '"  data-currencytype="' + rowdata.CurrencyType + '" onclick="withdrawalCreate(this)">申请提现</button>';
