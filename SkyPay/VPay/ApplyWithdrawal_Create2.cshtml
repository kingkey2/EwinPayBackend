                    "title": "幣別", "data": "CurrencyType"
                },
                {
                    "title": "通道代码", "data": "ServiceType"
                },
                {
                    "title": "提领金额下限", "data": "MinLimit",
                    "render": function (data, display, rowdata) {
                        return toCurrency(data);
                    }
                },
                {
                    "title": "提领金额上限", "data": "MaxLimit",
                    "render": function (data, display, rowdata) {
                        return toCurrency(data);
                    }
                },
                {
                    "title": "手续费", "data": "Charge",
