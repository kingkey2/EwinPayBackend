    function modifyRowValueByFirstConfirm(withdrawSerial, withdrawdata) {

        var index = -1;
        var row = Table.row(function (idx, data, node) {
            if (data.WithdrawSerial == withdrawSerial) {
                index = idx;
                return this;
            }
        });
        if (index != -1) {

            Table.row(row).data(withdrawdata);

            Table.row(row).data(Table.row(row).data()).draw(false);

        }

    }

