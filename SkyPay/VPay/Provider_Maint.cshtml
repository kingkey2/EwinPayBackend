<script>
    var ProviderServiceTable;
    var WithdrawLimitTable;
    var GPayRelationTable;
    var providerdata = null;
    var isCreatedProviderServiceTable = false;
    var isCreatedWithdrawLimitTable = false;
    var isCreatedGPayRelationTable = false;
    var providerCompanyID = 0;
    $(document).ready(function () {
           if ('@CheckPermission' == 'false') {
            parent.errorAlertCallback("权限不足", function () {
                parent.GoPage("Home");
                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
            });
        }

        pageLoad();
    });

    function pageLoad() {
        
if ('@CheckLoginPermission' == 'false') {
                location.href = "Login.cshtml";
            }

        AdminObject = parent.AdminObject;
        api = parent.api;

        if (AdminObject.CompanyType == 4) {
            $('#btnProviderCreate').hide();
            $('#btnProviderWithdrawLimitCreate').hide();
        }

        getProviderCodeResult();

        $('#table_ProviderService tbody').on('click', 'tr', function () {
            if ($(this).hasClass('selected')) {
                $(this).removeClass('selected');
            } else {
                ProviderServiceTable.$('tr.selected').removeClass('selected');
                $(this).addClass('selected');
            }
        });

        $('#table_ProviderWithdrawLimit tbody').on('click', 'tr', function () {
            if ($(this).hasClass('selected')) {
                $(this).removeClass('selected');
            } else {
                WithdrawLimitTable.$('tr.selected').removeClass('selected');
                $(this).addClass('selected');
            }
        });
        //新增供應商
        $("#btnProviderCreate").on("click", function () {
            var data = {
                backID: window.frameElement.id.split('_')[1]
            }

            var tmpdata = {
                type: "Provider_Create",
                data: data
            };
            sessionStorage.setItem("TempData", JSON.stringify(tmpdata));
            parent.GoPage("Provider_Create", "供应商新增", "Provider_Create.cshtml");

        })
        //編輯供應商
        $("#btnProviderEdit").on("click", function () {
            var modifyProviderData = {};

            modifyProviderData.ProviderCode = $('#b_ProviderCode').text();
            modifyProviderData.ProviderName = $('#b_ProviderName').text();
            modifyProviderData.Introducer = $('#b_Introducer').text();
            modifyProviderData.ProviderUrl = $('#b_ProviderUrl').text();
            modifyProviderData.MerchantCode = $('#b_MerchantCode').text();
            modifyProviderData.MerchantKey = $('#b_MerchantKey').text();
            modifyProviderData.NotifySyncUrl = $('#b_NotifySyncUrl').text();
            modifyProviderData.NotifyAsyncUrl = $('#b_NotifyAsyncUrl').text();
            modifyProviderData.ProviderAPIType = $('#b_ProviderAPIType').text();
            modifyProviderData.WithdrawRate = $('#b_WithdrawRate').text();
            modifyProviderData.forCompanyID = providerCompanyID;
            
            if ($('#b_CollectType').text() == "一般") {
                modifyProviderData.CollectType = 0;
            } else {
                modifyProviderData.CollectType = 1;
            }
            
            var backID = window.frameElement.id.split('_')[1];
            parent.CreateSessionStorage("Provider_Maint", modifyProviderData, `供应商修改(${modifyProviderData.ProviderName})`, "Provider_Update.cshtml", backID, "Provider_Update_" + modifyProviderData.ProviderCode);


        })
        parent.wrapperFadeOut();
    }

    //新增供應商渠道
    $("#btnProviderServiceCreate").on("click", function () {
        var data = {
            ProviderCode: $('#select_providercode').val(),
            ProviderName: $('#b_ProviderName').text(),
            backID: window.frameElement.id.split('_')[1]
        }

        var tmpdata = {
            type: "ProviderServiceCreate",
            data: data
        };

        sessionStorage.setItem("TempData", JSON.stringify(tmpdata));
        parent.GoPage("ProviderServiceCreate", "支付方式新增", "ProviderService_Create.cshtml");
    })
    //編輯供應商渠道
    $("#btnProviderServiceEdit").on("click", function () {
        if (ProviderServiceTable == undefined) {
            parent.warningAlert("尚未选择供应商渠道");
        }
        else if (ProviderServiceTable.row(".selected").data() == undefined) {
            parent.warningAlert("请先选择要编辑的资料");
        } else {

            var backID = window.frameElement.id.split('_')[1];
            ProviderServiceTable.row(".selected").data().ProviderName = $('#b_ProviderName').text();
            parent.CreateSessionStorage("ProviderService_Maint", ProviderServiceTable.row(".selected").data(), `支付方式修改(${ProviderServiceTable.row(".selected").data().ServiceTypeName})`, "ProviderService_Update.cshtml", backID, "ProviderService_Update_" + ProviderServiceTable.row(".selected").data().ProviderCode + "_" + ProviderServiceTable.row(".selected").data().ServiceType);
        }

    })
    //停用供应商

    $("#btnProviderDisable").on("click", function () {

        swal({
            title: "是否要修改商户状态(停用/启用)？",
            type: "warning",
            showCancelButton: true,
            closeOnConfirm: true,
            confirmButtonText: "确定",
            cancelButtonText: "取消",
            confirmButtonColor: "#DD6B55",
        },
            function (isConfirm) {
                if (isConfirm) {
                    disableProvider($('#select_providercode').val());
                }
            });

    })

    function changeProviderServiceType(objbtn) {

        var providerCode = $('#select_providercode').val();
        var serviceType = $(objbtn).data("servicetype");
        var currencyType = $(objbtn).data("currencytype");

        disableProviderService(providerCode, serviceType, currencyType);

    }
    //停用供應商渠道
    $("#btnProviderServiceDisable").on("click", function () {
        if (ProviderServiceTable.row(".selected").data() == undefined) {
            parent.warningAlert("请先选择要修改的资料");
        } else {
            swal({
                title: "是否要(停用/启用)？",
                type: "warning",
                showCancelButton: true,
                closeOnConfirm: true,
                confirmButtonText: "确定",
                cancelButtonText: "取消",
                confirmButtonColor: "#DD6B55",
            },
                function (isConfirm) {
                    if (isConfirm) {
                        disableProviderService(ProviderServiceTable.row(".selected").data().ProviderCode, ProviderServiceTable.row(".selected").data().ServiceType, ProviderServiceTable.row(".selected").data().CurrencyType);
                    }
                });
        }
    })
    //新增供應商代付限額
    $("#btnProviderWithdrawLimitCreate").on("click", function () {
        var data = {
            ProviderCode: $('#select_providercode').val(),
            ProviderName: $('#b_ProviderName').text(),
            backID: window.frameElement.id.split('_')[1]
        }

        var tmpdata = {
            type: "ProviderWithdrawLimitCreate",
            data: data
        };
        sessionStorage.setItem("TempData", JSON.stringify(tmpdata));
        parent.GoPage("ProviderWithdrawLimitCreate", "供应商代付限额新增", "ProviderWithdrawLimit_Create.cshtml");

    })
    //編輯供應商代付限額
    $("#btnProviderWithdrawLimitEdit").on("click", function () {
        if (WithdrawLimitTable == undefined) {
            parent.warningAlert("尚未有供应商代付资料");
        }
        else if (WithdrawLimitTable.row(".selected").data() == undefined) {
            parent.warningAlert("请先选择要编辑的资料");
        } else {
            WithdrawLimitTable.row(".selected").data().ProviderName = $('#b_ProviderName').text();

            var backID = window.frameElement.id.split('_')[1];
            WithdrawLimitTable.row(".selected").data().ProviderName = $('#b_ProviderName').text();
            parent.CreateSessionStorage("ProviderWithdrawLimit_Maint", WithdrawLimitTable.row(".selected").data(), `代付限额修改(${WithdrawLimitTable.row(".selected").data().ProviderName})`, "ProviderWithdrawLimit_Update.cshtml", backID, "ProviderWithdrawLimit_Update_" + WithdrawLimitTable.row(".selected").data().ProviderCode);
        }
    })


    function disableProviderService(providerCode, serviceType, currencyType) {
        api.disableProviderServiceResult(providerCode, serviceType, currencyType, function (success, obj) {
            if (obj.ResultCode == 0) {
                TempData = {
                    "type": "afterModifyProvider",
                    "data": providerCode
                }
                sessionStorage.setItem("TempData", JSON.stringify(TempData));
                parent.successAlert(function () {
                    parent.refreshPage(window.frameElement.id.split('_')[1]);
                });
            } else {

                switch (obj.ResultCode) {
                    case 4:
                        parent.errorAlertCallback("权限不足", function () {
                            parent.ChangePage("Home");
                            parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                        });
                        break;

                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;

                    default:
                        parent.warningAlert("其他错误");
                        break;
                }

            }
        });
    }

    function disableProvider(providerCode) {
        api.disableProvider(providerCode, function (success, obj) {
            if (obj.ResultCode == 0) {
                TempData = {
                    "type": "afterModifyProvider",
                    "data": providerCode
                }
                sessionStorage.setItem("TempData", JSON.stringify(TempData));
                parent.successAlert(function () {
                    parent.refreshPage(window.frameElement.id.split('_')[1]);
                });
            } else {

                switch (obj.ResultCode) {
                    case 4:
                        parent.errorAlertCallback("权限不足", function () {
                            parent.ChangePage("Home");
                            parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                        });
                        break;

                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    case 9:
                        parent.warningAlert(obj.Message);
                        break;
                    default:
                        parent.warningAlert("其他错误");
                        break;
                }

            }
        });
    }

    function getProviderCodeResult() {
        api.getProviderCodeResult(function (success, obj) {
            if (isCreatedProviderServiceTable) {
                ProviderServiceTable.clear().draw();
            }

            if (isCreatedGPayRelationTable) {
                GPayRelationTable.clear().draw();
            }

            if (obj.ResultCode == 0) {
                providerdata = obj.ProviderTypes;
                createProviderSelect();
            } else {
                var message = ""
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    case 5:
                        //message = "沒有資料";
                        //warningAlert(message);
                        break;
                    default:
                        //message = "其他錯誤";
                        //warningAlert(message);
                        break;
                }


            }
        });
    }

    function createProviderSelect() {

        if (providerdata) {

            var selectedproviderdata;

            $.each(providerdata, function (i, item) {
                $("#select_providercode").append('<option value="' + providerdata[i].ProviderCode + '">' + providerdata[i].ProviderName + '</option>');
            })

            //確認是否從新增或修改頁面完成後進入,如果是的話找出剛剛修改後的供應商資料
            if (window.sessionStorage["TempData"]) {
                var tempData = JSON.parse(window.sessionStorage["TempData"]);
                if (tempData.type == "afterModifyProvider") {
                    selectedproviderdata = providerdata.find(function (d) { return d.ProviderCode == tempData.data });
                } else {
                    selectedproviderdata = providerdata[0];
                }
            } else {
                selectedproviderdata = providerdata[0];
            }


            $("#select_providercode").val(selectedproviderdata.ProviderCode);

            $('#select_providercode').selectpicker('refresh');

            if (selectedproviderdata) {
                providerCompanyID = selectedproviderdata.forCompanyID;
                $('#b_ProviderCode').text(selectedproviderdata.ProviderCode);
                $('#b_ProviderName').text(selectedproviderdata.ProviderName);
                $('#b_Introducer').text(selectedproviderdata.Introducer);
                $('#b_ProviderUrl').text(selectedproviderdata.ProviderUrl);
                $('#b_MerchantCode').text(selectedproviderdata.MerchantCode);
                $('#b_MerchantKey').text(selectedproviderdata.MerchantKey);
                $('#b_NotifySyncUrl').text(selectedproviderdata.NotifySyncUrl);
                $('#b_NotifyAsyncUrl').text(selectedproviderdata.NotifyAsyncUrl);
                $('#b_ProviderAPIType').text(selectedproviderdata.ProviderAPIType);
                $('#b_WithdrawRate').text(selectedproviderdata.WithdrawRate);
                $('#b_ProviderAPITypeStr').text(checkedProviderAPIType(selectedproviderdata.ProviderAPIType));
                if (selectedproviderdata.ProviderState == 0) {
                    $('#b_ProviderState').text('启用');
                    $('#b_ProviderState').css('color', 'blue');
                } else {
                    $('#b_ProviderState').text('停用');
                    $('#b_ProviderState').css('color', 'red');
                }

                if (selectedproviderdata.CollectType == 0) {
                    $('#b_CollectType').text('一般');
                    $('#b_CollectType').css('color', 'blue');
                } else {
                    $('#b_CollectType').text('專屬廠商');
                    $('#b_CollectType').css('color', 'red');
                }

         
               
                

                $('#divProviderDetail').show();
                getProviderServiceTableResult(selectedproviderdata.ProviderCode);
                getProviderWithdrawLimitResult(selectedproviderdata.ProviderCode);
                getGPayRelationTableResult(selectedproviderdata.ProviderCode);
            }
        }
    }

