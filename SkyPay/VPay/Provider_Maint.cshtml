    function deleteProviderService(objbtn) {

        var providerCode = $('#select_providercode').val();
        var serviceType = $(objbtn).data("servicetype");
        var currencyType = $(objbtn).data("currencytype");
        swal({
            title: "是否确定删除支付通道？",
            type: "warning",
            showCancelButton: true,
            closeOnConfirm: true,
            confirmButtonText: "确定",
            cancelButtonText: "取消",
            confirmButtonColor: "#DD6B55",
        },
            function (isConfirm) {
                if (isConfirm) {
                    api.deleteProviderService(providerCode, serviceType, currencyType, function (success, obj) {

                        if (obj.ResultCode == 0) {
                            TempData = {
                                "type": "afterModifyProvider",
                                "data": providerCode
                            }
                            sessionStorage.setItem("TempData", JSON.stringify(TempData));
                            parent.successAlert(function () {
                                parent.refreshPage(window.frameElement.id.split('_')[1]);
                            });
                        } else {

                            switch (obj.ResultCode) {
                                case 4:
                                    parent.errorAlertCallback("权限不足", function () {
                                        parent.ChangePage("Home");
                                        parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                    });
                                    break;

                                case 7:
                                    parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                    break;

                                default:
                                    parent.warningAlert("其他错误");
                                    break;
                            }

                        }
                    });
                }
            });
    }

    function getProviderServiceTableResult(providercode) {
        api.getProviderServiceResult(providercode, function (success, obj) {

            if (isCreatedProviderServiceTable) {
                ProviderServiceTable.clear().draw();
            }

            if (obj.ResultCode == 0) {
                if (!isCreatedProviderServiceTable) {
                    isCreatedProviderServiceTable = true;
                    CreateProviderServiceTable(obj.ProviderServices);

                } else {
                    ProviderServiceTable.rows.add(obj.ProviderServices).draw();

                }
                parent.wrapperFadeOut();
            } else {
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    default:
                        //warningAlert("其他錯誤");
                        break;
                }
                parent.wrapperFadeOut();
            }
        });
    }

