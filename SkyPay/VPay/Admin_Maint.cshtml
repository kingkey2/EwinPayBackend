    <div id="GooleKeyContent">

    </div>
</body>
    <script>
    var Table;
    var tmpdata;
    var isCreatedTable = false;

    $(document).ready(function () {
        pageLoad();
    });
        function pageLoad() {
            
          if ('@CheckLoginPermission' == 'false') {
                location.href = "Login.cshtml";
            }

          if ('@CheckPermission' == 'false') {
            parent.errorAlertCallback("权限不足", function () {
                parent.GoPage("Home");
                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
            });
        }

        AdminObject = parent.AdminObject;
        api = parent.api;
        //建立下線選單
        parent.layout_getCompanyTableResult();

        //確認是否從新增或修改頁面完成後進入
        if (window.sessionStorage["TempData"]) {
            var tempData = JSON.parse(window.sessionStorage["TempData"]);
            if (tempData.type == "afterModifyAdmin") {
                $('#companyLabel').text(tempData.data.CompanyName);
                $('#companyLabel').attr("data-id", tempData.data.CompanyID);
            } else {
                $('#companyLabel').text(AdminObject.CompanyName);
                $('#companyLabel').attr("data-id", AdminObject.forCompanyID);
            }
        } else {
            $('#companyLabel').text(AdminObject.CompanyName);
            $('#companyLabel').attr("data-id", AdminObject.forCompanyID);
        }


        if (AdminObject.CompanyType == 1) {
            $('#btn_selectCompany').hide();
        }

        getAdminTableResult();

    };

    function createAdminTable(data) {
        Table = $("#table_AdminTable").DataTable({
            data: data,
            columns: [
                { "title": "帐号", "data": "LoginAccount" },
                {
                    "title": "状态", "data": "AdminState",
                    "render": function (data) {
                        if (data == 0) {
                            return '<span style="color:blue;">启用</span>';
                        } else {
                            return '<span style="color:red;">停用</span>';
                        }
                    }
                },
                { "title": "", "data": "forAdminRoleID", "visible": false },
                { "title": "姓名", "data": "RealName" },
                { "title": "描述", "data": "Description" },
                { "title": "角色名称", "data": "RoleName" },
                { "title": "", "data": "AdminID", "visible": false },
                { "title": "", "data": "forCompanyID", "visible": false },
                { "title": "", "data": "AdminState", "visible": false },
                { "title": "建立日期", "data": "CreateDate2" },
                {
                    "title": "功能", "data": "LoginAccount",
                    "render": function (data, display, rowdata) {
                        if (rowdata.CompanyType == 0 || rowdata.CompanyType == 3) {
                            return '<button style="margin-left:5px" data-loginaccount="' + data+'" onclick="getGoogleKey(this)">谷歌验证码</button>';
                        } else {
                            return '';
                        }
                     
                    }
                }
            ],
            rowCallback: function (row, data) { },
            filter: true,
            info: false,
            ordering: true,
            processing: true,
            retrieve: true,
            selected: true,
            scrollX: true,
            autoWidth: false,
            language: parent.chineseTableLang
        });

        $('#table_AdminTable tbody').on('click', 'tr', function () {
            if ($(this).hasClass('selected')) {
                $(this).removeClass('selected');
            } else {
                Table.$('tr.selected').removeClass('selected');
                $(this).addClass('selected');
            }
        });
    }

    function getGoogleKey(objbtn) {
            var loginAccount = $(objbtn).data('loginaccount');
            parent.wrapperFadeIn();
            $('#GooleKeyContent').empty();
            api.getGoogleQrCodeByAdmin(loginAccount,function (success, obj) {
                if (success) {
                    if (obj.ResultCode == 0) {
                        tmpdata = obj.GoogleQrCodeResult;
                        createGoogleKeyModal();
                        $('#modal_LoginAccount').text(loginAccount);
                        if (!obj.GoogleQrCodeResult.IsCreated) { 
                            $('#qrCodeImg').attr('src', obj.GoogleQrCodeResult.ImageUrl);
                            $('#qrCodeImg').parent().show();
                        }

                        $('#GooleKeyModal').modal('show');
                    }
                    else {
                        switch (obj.ResultCode) {
                            case 5:
                                parent.warningAlert("账号不存在");
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            default:
                                parent.warningAlert("其他错误");
                                break;
                        }
                    }
                    parent.wrapperFadeOut();
                } else {
                    parent.wrapperFadeOut();
                    parent.warningAlert("其他错误");
                }
            });
        }

        function createGoogleKeyModal() {
 
        var dochtml = ' <div class="modal fade" style="background-color: rgba(0, 0, 0, 0.4);" data-backdrop=false id="GooleKeyModal" tabindex="-1" role="dialog" aria-hidden="true">' +
                '<div class="modal-dialog modal-sm" role="document">' +
                '<div class="modal-content">' +
                '<div class="modal-header">' +
                ' <h4 class="title">谷歌验证</h4>' +
                '</div>' +
                '<div class="modal-body">' +
                '<span style="display:block">账号:<span id="modal_LoginAccount" style="margin-left:5px"></span></span>' +
                '<span style="display:none"><img id="qrCodeImg" /></span>' +
                '<span style="display:block">动态密码:<input type="text" id="model_UserKey" style="margin-left:5px"></span>' +
                '</div>' +
                '<div class="modal-footer">' +
                '<button onclick="saveGooleKey()" type="button" class="btn btn-primary btn-round waves-effect">绑定/解除</button>' +
                '<button onclick="contentcancel()" type="button" class="btn btn-primary btn-round waves-effect">取消</button>' +
                '</div>' +
                '</div>' +
                '</div>' +
                '</div>';
            $('#GooleKeyContent').append(dochtml);
        }

        function saveGooleKey() {
            var loginaccount = $('#modal_LoginAccount').text();
            if (tmpdata) {
                if ($('#model_UserKey').val().trim() == "") {
                    parent.warningAlert("尚未填写动态密码");
                    return;
                }

                var data = {
                    UserKey: $('#model_UserKey').val().trim(),
                    GoogleKey: tmpdata.GoogleKey,
                    LoginAccount: loginaccount
                }

                if (tmpdata.IsCreated) {     
                    api.unsetGoogleQrCodeByAdmin(data, function (success, obj) {
                        if (obj.ResultCode == 0) {
                            parent.successAlert3("设定完成",function () {
                                contentcancel();
                            });
                        } else {
                            var message = ""
                            switch (obj.ResultCode) {

                                case 7:
                                    parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                    break;
                                case 6:
                                    message = "动态密码有误";
                                    break;
                                default:
                                    message = "其他错误";
                                    break;
                            }

                            parent.warningAlert(message);
                        }
                    });
                } else {
 
                    api.setGoogleQrCodeByAdmin(data, function (success, obj) {
                        if (obj.ResultCode == 0) {
                            parent.successAlert3("设定完成", function () {
                                contentcancel();
                            });
                        } else {
                            var message = ""
                            switch (obj.ResultCode) {

                                case 7:
                                    parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                    break;
                                case 6:
                                    message = "动态密码有误";
                                    break;
                                default:
                                    message = "其他错误";
                                    break;
                            }

                            parent.warningAlert(message);
                        }
                    });
                }

            } else {
                parent.warningAlert("账号资讯有误");
            }
        }

        function contentcancel() {
            $('#GooleKeyModal').modal('hide');
        }

    function getAdminTableResult() {
        var companyid = $('#companyLabel').attr("data-id");
        api.getAdminTableResult(companyid, function (success, obj) {
            if (isCreatedTable) {
                Table.clear().draw();
            }
            if (obj.ResultCode == 0) {
                if (!isCreatedTable) {
                    isCreatedTable = true;
                    //建立資料顯示Table
                    createAdminTable(obj.AdminResults);
                } else {
                    Table.rows.add(obj.AdminResults).draw(); // Add new data
                }

                parent.wrapperFadeOut()
            } else {
                var message = ""
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    case 5:

                        break;
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    default:
                        message = "其他错误";
                        parent.warningAlert(message);
                        break;
                }
                parent.wrapperFadeOut();


            }
        });

    }

    $('#btnadd').click(function () {

        var data = {
            CompanyName: $('#companyLabel').text(),
            CompanyID: $('#companyLabel').attr("data-id"),
            backID: window.frameElement.id.split('_')[1]
        }

        var tmpdata = {
            type: "Admin_Add",
            data: data
        };
        sessionStorage.setItem("TempData", JSON.stringify(tmpdata));
        parent.GoPage("Admin_Add", "后台帐号新增", "Admin_Add.cshtml");

    })

    $('#btndisable').click(function () {
        if (Table.row(".selected").data() == undefined) {
            parent.warningAlert("请先选择要修改的资料");
        } else {
            swal({
                title: "是否要停用？",
                type: "warning",
                showCancelButton: true,
                closeOnConfirm: true,
                confirmButtonText: "确定",
                cancelButtonText: "取消",
                confirmButtonColor: "#DD6B55"
            },
                function (isConfirm) {
                    if (isConfirm) {
                        disableAdmin(Table.row(".selected").data().AdminID, Table.row(".selected").data().forCompanyID);
                    }
                });
        }

    })

    function disableAdmin(adminID, companyID) {
        api.disableAdmin(adminID, companyID, function (success, obj) {
            if (obj.ResultCode == 0) {
                parent.successAlert(function () {
                    parent.refreshPage(window.frameElement.id.split('_')[1]);
                });
            } else {

                switch (obj.ResultCode) {
                    case 4:
                        parent.errorAlertCallback("权限不足", function () {
                            parent.ChangePage("Home");
                            parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                        });
                        break;

                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;

                    default:
                        parent.warningAlert("其他错误");
                        break;
                }

            }
        });
    }


    //編輯
    $("#btnedit").on("click", function () {

        if (Table.row(".selected").data() != undefined) {
            var backID = window.frameElement.id.split('_')[1];
            Table.row(".selected").data().CompanyName = $('#companyLabel').text();
            parent.CreateSessionStorage("Admin_Maint", Table.row(".selected").data(), `后台账号修改(${Table.row(".selected").data().LoginAccount})`, "Admin_Update.cshtml", backID, "Admin_Update_" + Table.row(".selected").data().AdminID);
        } else {
            parent.warningAlert("请先选择要修改的资料");
        }

    })

    //選擇供應商事件
    $('#btn_selectCompany').click(function () {
        parent.showSelectCompany_menu($('#companyLabel'), window.frameElement.id);
    });
    //選擇供應商後callback事件
    function btnSaveselectcompanyCallback() {
        getAdminTableResult();
    }



    </script>
