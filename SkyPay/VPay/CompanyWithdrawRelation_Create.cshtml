<script>
    var checkServiceTypeData = null;
    var CompanyID;
    var tmpdata;
    var ParentCompanyID;
    var tmpParentWithdrawLimits;

    var EditData = parent.CheckTempDataType("CompanyWithdrawRelationCreate");

    $(document).ready(function () {
        pageLoad();
    });

    function pageLoad() {
        if ('@CheckLoginPermission' == 'false') {
                location.href = "Login.cshtml";
            }

        AdminObject = parent.AdminObject;
        api = parent.api;
        tmpdata = EditData.data;
  
        CompanyID = tmpdata.CompanyID;
        ParentCompanyID = tmpdata.ParentCompanyID;
   
        getApiWithdrawLimit();

        new AutoNumeric.multiple('.thousand-symbols', 'floatPos', { allowDecimalPadding: false, decimalPlaces: 4, modifyValueOnWheel: false });
  
        $('.thousand-symbols').unbind('focus');

        parent.wrapperFadeOut();

        $('#btnSave').on('click', function () {

            if (!$('#select_CurrencyType').val()) {
                parent.warningAlert("尚未选择币别");
                return;
            }
       
            if ($("#input_Charge").val().trim() == "") {
                parent.warningAlert("请先输入手续费");
                return;
            }

            if ($("#input_MinLimit").val().trim() == "") {
                parent.warningAlert("请先输入单笔交易最低金额");
                return;
            }

            if ($("#input_MaxLimit").val().trim() == "") {
                parent.warningAlert("请先输入单笔交易最高金额");
                return;
            }

            if (toNumber($("#input_MinLimit").val()) > toNumber($("#input_MaxLimit").val())) {
                parent.warningAlert("单笔交易最低金额不可超过单笔交易最高金额");
                return;
            }

            //if (ParentCompanyID == 0) {
                if ($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").length > 0) {
                    var Charge = 0;
                    var MinLimit = 0;
                    var MaxLimit = 0;
                    for (var i = 0; i < $("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").length; i++) {
                        if (i == 0) {
                            Charge = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_Charge').text());
                            MinLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MinLimit').text());
                            MaxLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MaxLimit').text());

                        } else {
                            if (Charge < toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_Charge').text())) {
                                Charge = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_Charge').text());
                            }

                            if (MinLimit < toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MinLimit').text())) {
                                MinLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MinLimit').text());
                            }
                            if (MaxLimit > toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MaxLimit').text())) {
                                MaxLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MaxLimit').text());
                            }

                        }
                    }

                    var tmpCharge = $("#input_Charge").val().trim();
                    var tmpMinLimit = $("#input_MinLimit").val().trim();
                    var tmpMaxLimit = $("#input_MaxLimit").val().trim();

                    if (toNumber(tmpCharge) < Charge) {
                        parent.warningAlert("单笔手续费小于供应商手续费");
                        return;
                    }

                    if (toNumber(tmpMinLimit) < MinLimit) {
                        parent.warningAlert("单笔交易最低金额小于供应商最低金额");
                        return;
                    }
                    if (toNumber(tmpMaxLimit) > MaxLimit) {
                        parent.warningAlert("单笔交易最高金额大于供应商最高金额");
                        return;
                    }

                }
            //} else {
            //    var parentWithdrawLimit = tmpParentWithdrawLimits.find(data => data.CurrencyType == $('#select_CurrencyType').val());

            //    if (parentWithdrawLimit) {
            //        if (parentWithdrawLimit.Charge > $("#input_Charge").val().trim()) {
            //            warningAlert("单笔手续费 需超过或等于上线额");
            //            return;
            //        }

            //        if (toNumber($("#input_MinLimit").val()) < parentWithdrawLimit.MinLimit) {
            //            warningAlert("单笔交易最低金额 需超过或等于上线额");
            //            return;
            //        }

            //        if (toNumber($("#input_MaxLimit").val()) > parentWithdrawLimit.MaxLimit) {
            //            warningAlert("单笔交易最高金额 不可超过上线额");
            //            return;
            //        }
            //    } else {
            //        warningAlert("请先设定上线代付限额");
            //        return;
            //    }
               

         
            //}
          
            var lstProviderCode = [];
            for (var i = 0; i < $("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").length; i++) {
                var provider = new Object();
                var providerCode = $("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).attr("id");
                provider.ProviderCode = $("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).attr("id");
                provider.Weight = $("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_Weight').val();
                lstProviderCode.push(provider);
    
            }
            
            var companyID = CompanyID;
            var charge = $("#input_Charge").val().trim();
            var minLimit = $("#input_MinLimit").val().trim();
            var maxLimit = $("#input_MaxLimit").val().trim();
            var currencyType = $("#select_CurrencyType").val();

            api.insertGPayWithdrawRelation(companyID, charge, minLimit, maxLimit, lstProviderCode, currencyType, function (success, obj) {
                if (obj.ResultCode == 0) {
                    TempData = {
                        "type": "afterModifyCompany",
                        "data": companyID
                    }
                    sessionStorage.setItem("TempData", JSON.stringify(TempData));
                    parent.successAlert(function () {
                        parent.refreshPage(EditData.data.backID);
                        parent.CloseActivePage(window.frameElement.name);
                    });
                } else {
                    var message = ""
                    switch (obj.ResultCode) {
                        case 4:
                            parent.errorAlertCallback("权限不足", function () {
                                parent.ChangePage("Home");
                                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                            });
                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        case 8:
                            parent.warningAlert("资料已存在");
                            break;
                        default:
                            parent.warningAlert("其他错误");
                            break;
                    }
                }
            });

        })

        $('#btnCancel').on('click', function () {

            TempData = {
                "type": "afterModifyCompany",
                "data": CompanyID
            }

            sessionStorage.setItem("TempData", JSON.stringify(TempData));
            parent.ChangePage(EditData.data.backID);
            parent.CloseActivePage(window.frameElement.name);
        })
    }
    function getApiWithdrawLimit() {
        $('#providerServiceSet').show();
        $("#divGPayRelationDetail").html("&nbsp;&nbsp;&nbsp;&nbsp;尚未设定供应商");
        api.getApiWithdrawLimit(0, function (s, obj) {
            if (s) {
                CreateCurrencySelect();
                if (obj.ResultCode == 0) {
                    CreateGPayWithdrawRelation(obj.ApiWithdrawLimitResults);
                } else {
                    var message = ""
                    switch (obj.ResultCode) {
                        case 4:
                            parent.errorAlertCallback("权限不足", function () {
                                parent.ChangePage("Home");
                                parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                            });
                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        case 5:
                            $("#divGPayRelationDetail").html("&nbsp;&nbsp;&nbsp;&nbsp;尚未设定供应商");
                            break;
                        default:
                            break;
                    }
                }
            }
           
        });

    }

    function getParentApiWithdrawLimit() {

        api.getCompanyWithdrawRelationResult(ParentCompanyID, function (success, obj) {

            if (obj.ResultCode == 0) {
                tmpParentWithdrawLimits = obj.WithdrawLimits;
                CreateCurrencySelect();
                parent.wrapperFadeOut();
            } else {
              
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    case 5:
                        parent.warningAlert("上线尚未设定代付限额");
                        break;
                    default:
                        //warningAlert("其他錯誤");
                        break;
                }
                CreateCurrencySelect();
                parent.wrapperFadeOut();
            }
        });

    }
   
    function CreateGPayWithdrawRelation(data) {
        var strGPayRelationDetail = "";
     
            $.each(data, function (i, item) {

                strGPayRelationDetail += '<div class="col-lg-2 col-md-6">' +
                    '<div class="card" >' +
                    '<div class="header" style="padding-bottom:0">';

                strGPayRelationDetail += '<div class="checkbox inlineblock delete_all"><input class="RelationModel_checkbox" id="' + data[i].ProviderCode + '" type="checkbox"><label for="' + data[i].ProviderCode + '">' + data[i].ProviderName + ' </label></div>';

                strGPayRelationDetail += '</div > ' +
                    '<div class="body" style="padding-top:0"> ' +
                    '<p class="text-muted">单笔手续费： <span class="RelationModel_Charge">' + data[i].Charge + '</span></p>' +
                    '<p class="text-muted">单笔交易最低金额： <span class="RelationModel_MinLimit">' + toCurrency(data[i].MinLimit) + '</span></p>' +
                    '<p class="text-muted">单笔交易最高金额： <span class="RelationModel_MaxLimit">' + toCurrency(data[i].MaxLimit) + '</span></p>' +
                    '<p class="text-muted">币别： <span class="RelationModel_CurrencyType">' + data[i].CurrencyType + '</span></p>' +
                    '<p class="text-muted" style="display:none;">权重： <input data-rule="currency" style="width:50%;" class="form-control RelationModel_Weight" type="text" value=1></p>';
               
                    strGPayRelationDetail += '</div ></div ></div > ';

        })

        $("#divGPayRelationDetail").html(strGPayRelationDetail);
        new AutoNumeric.multiple('.RelationModel_Weight', 'integerPos', { allowDecimalPadding: false, decimalPlaces: 0, minimumValue: 1, maximumValue: 100 });

        $('.RelationModel_checkbox').change(function () {
    
            var Charge = "";
            var MinLimit = "";
            var MaxLimit = "";
            //$("input[name='RelationModel_checkbox']").each(function () {
            //    $(this).prop("checked", false);
            //});
            $('.RelationModel_checkbox').prop('checked', false);
            $(this).prop('checked', true);
            if ($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").length > 0) {
                for (var i = 0; i < $("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").length; i++) {
                    if (i == 0) {
                        Charge = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_Charge').text());
                        MinLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MinLimit').text());
                        MaxLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MaxLimit').text());

                    } else {
                        if (Charge < toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_Charge').text())) {
                            Charge = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_Charge').text());
                        }
                      
                        if (MinLimit < toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MinLimit').text())) {
                            MinLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MinLimit').text());
                        }
                        if (MaxLimit > toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MaxLimit').text())) {
                            MaxLimit = toNumber($("#divGPayRelationDetail input[class='RelationModel_checkbox']:checked").eq(i).parent().parent().parent().find('.RelationModel_MaxLimit').text());
                        }
                     
                    }
                }
            }
 
            $('#input_Charge').val(toCurrency(Charge));
            $('#input_MinLimit').val(toCurrency(MinLimit));
            $('#input_MaxLimit').val(toCurrency(MaxLimit));

        })
    }

    function CreateCurrencySelect() {
        api.getCurrency(function (s, obj) {
            if (obj.ResultCode == 0) {
                if (obj.Currencies.length > 0) {
                    $("#select_CurrencyType").children().remove();

                    $.each(obj.Currencies, function (i, item) {
                        $("#select_CurrencyType").append('<option value="' + obj.Currencies[i].CurrencyType + '">' + obj.Currencies[i].CurrencyType + '</option>');
                    })

                    $('#select_CurrencyType').selectpicker('refresh');
                }
                setNotifyLabelText($('#select_CurrencyType').val());
            }
            else {
                var message = ""
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                }
                parent.wrapperFadeOut();
            }
        });

        $("#select_CurrencyType").change(function () {
            setNotifyLabelText($(this).val());
        })
    }

    function setNotifyLabelText(currencytype) {
        var parentWithdrawLimit = tmpParentWithdrawLimits.find(data => data.CurrencyType == currencytype);
        if (parentWithdrawLimit == undefined) {
            $('#notifyCharge').text("(请先设定上线代付限额)");
            $('#notifyMinLimit').text("(请先设定上线代付限额)");
            $('#notifyMaxLimit').text("(请先设定上线代付限额)");
        } else {
            $('#notifyCharge').text("(手续费必须大于或等于" + parentWithdrawLimit.Charge + ")");
            $('#notifyMinLimit').text("(最低限额必须大于等于" + toCurrency(parentWithdrawLimit.MinLimit) + ")");
            $('#notifyMaxLimit').text("(最高限额必须小于等于" + toCurrency(parentWithdrawLimit.MaxLimit)+")");
        }
    }
</script>

