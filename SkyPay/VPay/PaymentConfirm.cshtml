    function modifyRowValue(paymentSerial, processStatus, paymentAmount) {
        var index = -1;
        var row = Table.row(function (idx, data, node) {
            if (data.PaymentSerial == paymentSerial) {
                index = idx;
                return this;
            }
        });
        if (index != -1) {

            Table.row(row).data().ProcessStatus = processStatus;
            Table.row(row).data().PaymentAmount = paymentAmount;

            Table.row(row).data(Table.row(row).data()).draw(false);
            Table.columns.adjust();
        }
    }

    function savePaymentConfirm() {
        var paymentSerial = $('#modal_PaymentSerial').text();
        var providerOrderID = $('#modal_ProviderOrderID').val().trim();
        var password = $('#modal_Password').val().trim();

        var patchAmount = $('#modal_PatchAmount').val().trim();

        if (patchAmount == "") {
            parent.warningAlert("尚未输入补单金额");
            return;
        }

        if (password == "") {
            parent.warningAlert("尚未输入密码");
            return;
        }

        $('#PaymentModal').modal('hide');

        swal({
            title: `单号:${paymentSerial},是否确认完成补单?`,
            type: "warning",
            showCancelButton: true,
            closeOnConfirm: true,
            confirmButtonText: "确定",
            cancelButtonText: "取消",
            confirmButtonColor: "#DD6B55",
        },
            function (isConfirm) {
                if (isConfirm) {
                    api.savePaymentConfirm(paymentSerial, providerOrderID, patchAmount, password, function (success, obj) {
                        if (obj.ResultCode == 0) {
                            parent.successAlert3("补单完成");
                            modifyRowValue(paymentSerial, obj.WithdrawalResult.Status, obj.WithdrawalResult.PaymentAmount);

                        } else {
                            switch (obj.ResultCode) {
                                case 2:
                                    parent.warningAlert("密码有误");
                                    break;
                                case 4:
                                    parent.errorAlertCallback("权限不足", function () {
                                        parent.ChangePage("Home");
                                        parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                    });
                                    break;
                                case 6:
                                    if (obj.WithdrawalResult.Status == -1) {
                                        parent.warningAlert(obj.WithdrawalResult.Message);
                                    } else {
                                        parent.warningAlert(obj.WithdrawalResult.Message);
                                        modifyRowValue(paymentSerial, obj.WithdrawalResult.Status, obj.WithdrawalResult.PaymentAmount);
                                    }

                                    break;
                                case 7:
                                    parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                    break;
                                default:
                                    message = "其他错误";
                                    parent.warningAlert(message);
                                    break;
                            }
                        }
                    });
                }
            });


    }

