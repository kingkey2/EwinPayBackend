    function PaymentReviewConfirm(data) {

        if (!isCreatedPaymentModal) {
            isCreatedPaymentModal = true;
            createPaymentModal();
        }

        $('#modal_CompanyName').text(data.CompanyName);
        $('#modal_PaymentSerial').text(data.PaymentSerial);
        $('#modal_ProviderName').text(data.ProviderName);
        $('#modal_CurrencyType').text(data.CurrencyType);
        $('#modal_Amount').text(toCurrency(data.OrderAmount));
        $('#modal_ServiceTypeName').text(data.ServiceTypeName);
        $('#modal_ProviderOrderID').val('');
        $('#modal_PatchAmount').val('');
        $('#modal_Password').val('');
        $('#PaymentModal').modal('show');
    }

