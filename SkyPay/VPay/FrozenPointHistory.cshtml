    function pictureChange(input) {
      
        if (input.files && input.files[0]) {
            if (Math.round(input.files[0].size / (1024 * 1024)) > 2) {
                parent.warningAlert("档案大小必须小于 2 MB");
                return false;
            }

            let reader = new FileReader();

            reader.readAsDataURL(input.files[0]);
            //將圖片轉成byte存下來供儲存用
            reader = new FileReader();
            reader.readAsArrayBuffer(input.files[0]);
            reader.onload = function (e) {

                var byteCurrencyPicture = e.target.result;
                var frozenID = $(input).data('frozenid');
                var type = $(input).data('type');
                var imageID = $(input).data('imageid');
                var imageData = Array.from(new Uint8Array(byteCurrencyPicture));
                var imageName = $(input).val().split("\\").pop().split(".").pop();
                $(input).parent().find('img').attr('src', e.target.result);

                api.updateFrozenPointimg(imageData, imageName, frozenID, type, imageID, function (success, obj) {
                    if (success) {
                        if (obj.ResultCode == 0) {
                            parent.successAlert(function () {
                                getTableResult();

                            });
                        } else {
                            switch (obj.ResultCode) {
                                case 4:
                                    parent.errorAlertCallback("权限不足", function () {
                                        parent.ChangePage("Home");
                                        parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                    });
                                    break;
                                case 6:
                                    message = "其他错误";
                                    parent.warningAlert(message);
                                    break;

                                case 7:
                                    parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                    break;
                                default:
                                    message = "其他错误";
                                    parent.warningAlert(message);
                                    break;
                            }
                        }
                    }
                })
            }
        }
    }

    function getProxyProviderGroup(providerCode) {
        parent.wrapperFadeIn();
        getSumFrozenPoint();
        api.getProxyProviderGroupFrozenPoint(providerCode, function (success, obj) {
            if (success) {
                parent.wrapperFadeOut();
                $('#ProxyGroupTable>tbody').empty();
                if (obj.ResultCode == 0) {

                    proxyProviderGroupData = obj.Results;
                    for (var i = 0; i < proxyProviderGroupData.length; i++) {
                        if (i == 0) {
                            var tmpTr = $('<tr></tr>');
                            tmpTr.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td style="font-size:15px;color:red;">${toCurrency(proxyProviderGroupData[i].FrozenAmount)}</td >`)
                        } else if (i % 3 == 0) {
                            $('#ProxyGroupTable>tbody').append(tmpTr);
                            tmpTr = $('<tr></tr>');
                            tmpTr.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td style="font-size:15px;color:red;">${toCurrency(proxyProviderGroupData[i].FrozenAmount)}</td >`)
                        } else {
                            tmpTr.append(`<td style="font-size:15px;">${proxyProviderGroupData[i].GroupName}</td><td style="font-size:15px;color:red;">${toCurrency(proxyProviderGroupData[i].FrozenAmount)}</td >`)
                        }

                        if (i == proxyProviderGroupData.length - 1) {
                            $('#ProxyGroupTable>tbody').append(tmpTr);
                        }
                    }
                }
                else {
                    var message = ""
                    switch (obj.ResultCode) {
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        case 5:

                            break;
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
