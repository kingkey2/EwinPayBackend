    $('#deteselect').change(function () {

        switch ($(this).val()) {
            case "0"://今日
                var start = moment().hour(00).minute(00);
                var end = moment().hour(23).minute(59);
                break;
            case "1"://昨日
                var start = moment().subtract(1, 'days').hour(00).minute(00);
                var end = moment().subtract(1, 'days').hour(23).minute(59);
                break;
            case "2"://过去7日
                var start = moment().subtract(6, 'days').hour(00).minute(00);
                var end = moment().hour(23).minute(59);
                break;
            case "3"://过去30日
                var start = moment().subtract(29, 'days').hour(00).minute(00);
                var end = moment().hour(23).minute(59);
                break;
            case "4"://这个月
                var start = moment().startOf('month').hour(00).minute(00);
                var end = moment().endOf('month').hour(23).minute(59);
                break;
            case "5"://上个月
                var start = moment().subtract(1, 'month').startOf('month').hour(00).minute(00);
                var end = moment().subtract(1, 'month').endOf('month').hour(23).minute(59);
                break;
            default:
        }
        $('#dateinput').data('daterangepicker').setStartDate(start);
        $('#dateinput').data('daterangepicker').setEndDate(end);

    })

    function getTableResult() {
        var dateinputval = $('#dateinput').val().split('-');
        var startDate = dateinputval[0];
        var endDate = dateinputval[1];
        var Status = $('#select_Type').val();
        var providercode = $('#select_providercode').val();
        var CompanyID = $('#select_Company').val();
        var PaymentSerial = $("#input_PaymentSerial").val();
        var groupID = $('#select_proxyProviderGroup').val();

        api.getFrozenPointHistory(startDate, endDate, PaymentSerial, CompanyID, providercode, Status, groupID, function (success, obj) {
            if (isCreatedTable) {
                Table.clear().draw();
            }

            if (obj.ResultCode == 0) {
                var companyFrozenAmount = 0;
                var providerFrozenAmount = 0;
                var frozendata = obj.Results;
                for (var i = 0; i < frozendata.length; i++) {
                    if (frozendata[i].Status == 0) {
                        companyFrozenAmount += frozendata[i].CompanyFrozenAmount;
                        providerFrozenAmount += frozendata[i].ProviderFrozenAmount;
                    }
                }
                $('#count1').text(toCurrency(providerFrozenAmount));
                $('#count2').text(toCurrency(companyFrozenAmount));
        
                if (!isCreatedTable) {
                    isCreatedTable = true;
                    createTable(frozendata);
                } else {
                    Table.rows.add(frozendata).draw();
                }
                parent.wrapperFadeOut();
            } else {
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    case 5:

                        parent.warningAlert("查无资料");
                        break;
                    default:
                        //warningAlert("其他錯誤");
                        break;
                }
                parent.wrapperFadeOut();
            }
        });
    }

    function createTable(data) {

        Table = $('#table_Table').DataTable({
            data: data,
            columns: [
                {
                    "title": "被冻结单号", "data": "forPaymentSerial"
                },
                {
                    "title": "商户", "data": "CompanyName"
                },
                {
                    "title": "渠道", "data": "ProviderName",
                    "render": function (data, display, rowdata) {

                        if (rowdata.GroupName != null) {
                            return `${data}/${rowdata.GroupName}`
                        }
                        return data;
                    }
                },
                {
                    "title": "银行卡资讯", "data": "BankCard",
                    "render": function (data, display, rowdata) {
                        var html = "";
                        if (!(rowdata.BankName == null || rowdata.BankName == "")) {
                            html += rowdata.BankName + "</br>";
                        }
                        if (!(rowdata.BankCard == null || rowdata.BankCard == "")) {
                            html += rowdata.BankCard + "</br>";
                        }
                        if (!(rowdata.BankCardName == null || rowdata.BankCardName == "")) {
                            html += rowdata.BankCardName + "</br>";
                        }

                        if (html != "") {
                            html = html.substring(0, html.length -5);
                        }


                        return "<span>" + html+ "</span>";
                    }
                },
                {
                    "title": "商户冻结金额", "data": "CompanyFrozenAmount",
                    "render": function (data, display, rowdata) {
                        return toCurrency(data);
                    }
                },
                {
                    "title": "渠道冻结金额", "data": "ProviderFrozenAmount",
                    "render": function (data, display, rowdata) {
                        return toCurrency(data);
                    }
                },
                {
                    "title": "状态", "data": "Status",
                    "render": function (data, display, rowdata) {
                        switch (data) {
                            case 0:
                                return '冻结';
                                break;
                            case 1:
                                return '解冻';
                                break;
                        }
                    }
                },
                {
                    "title": "备注", "data": "Description"
                },
                {

                    "title": "图片", "data": "ImageName", render: function (data, display, rowdata) {
                        if (rowdata.ImageName != null) {
                            var tmpHtml = '';
                            var imageNames = rowdata.ImageName.split(",").sort(function (a, b) {
                                var a_arr = a.split("_");
                                var a_date = a_arr[a_arr.length - 1];
                                var b_arr = b.split("_");
                                var b_date = b_arr[b_arr.length - 1];
                                return new Date(b_date) - new Date(a_date)
                            });
                            for (var i = 0; i < imageNames.length; i++) {
                                var imageName_arr = imageNames[i].split("_");
                                ImageID = imageName_arr[0];
                                ImageName = imageName_arr[1];
                                CreateDate = imageName_arr[2];

                                if (i == 0) {
                                    tmpHtml += '<i data-imageid="' + ImageID + '" data-frozenid="' + rowdata.FrozenID + '" class="zmdi zmdi-plus" style="margin-top: 7px;margin-right: 5px;" onclick="addImage(this)"></i><input data-type="update" onchange="pictureChange(this)"  data-imageid="' + ImageID + '"  data-frozenid="' + rowdata.FrozenID + '" type="file" style="border-radius:0px;border:0px;"  accept="image/gif, image/jpeg, image/png" /><div><span class="span_CreateDate" style="margin-right:10px;">' + CreateDate + '</span><button onclick="showImageScale(this)"  data-src="/Backend/GetFileFromSharedFolder.cshtml?FileType=FrozenPoint&FileName=' + ImageName + '">显示图片</button><button style="margin-left:10px;" data-imageid="' + ImageID + '" onclick="deleteImage(this)">删除</button></div>';
                                } else {
                                    tmpHtml += '<br/><input data-type="update" onchange="pictureChange(this)" data-imageid="' + ImageID + '"  data-frozenid="' + rowdata.FrozenID + '" type="file" style="border-radius:0px;border:0px;"  accept="image/gif, image/jpeg, image/png" /><div><span class="span_CreateDate" style="margin-right:10px">' + CreateDate + '</span><button onclick="showImageScale(this)"  data-src="/Backend/GetFileFromSharedFolder.cshtml?FileType=FrozenPoint&FileName=' + ImageName + '">显示图片</button><button style="margin-left:10px;" data-imageid="' + ImageID + '" onclick="deleteImage(this)">删除</button></div>';
                                }
                            }
                            return tmpHtml;
                        } else {
                            return '<i class="zmdi zmdi-plus" style="margin-top: 7px;margin-right: 5px;"  onclick="addImage(this)"></i><input  data-type="create" onchange="pictureChange(this)" data-frozenid="' + rowdata.FrozenID + '" type="file" style="border-radius:0px;border:0px;"  accept="image/gif, image/jpeg, image/png" /><div><button src ="" style="margin-top:5px;">未上传</button></div>';
                        }
                    }
                },
                {
                    "title": "操作时间", "data": "CreateDate2",
                    "render": function (data, display, rowdata) {
                        if (data != '0001-01-01T00:00:00') {
                            return data.replace(' ', '<br/>');
                        } else {
                            return '';
                        }
                    }
                },
                {
                    "title": "冻结者", "data": "FrozenAdminName"
                },
              
                {
                    "title": "解冻时间", "data": "UnFrozenDate",
                    "render": function (data, display, rowdata) {
                        if (data != '0001-01-01T00:00:00') {
                            return data.replace('T', '<br/>');
                        } else {
                            return '';
                        }
                    }
                },
                {
                    "title": "解冻者", "data": "UnFrozenAdminName"
                },
                {
                    "title": "操作",
                    "data": "FrozenID",
                    "render": function (data, display, rowdata) {

                        let retValue;
                        if (rowdata.Status == 0)
                            retValue = '<button type="button" data-frozenid=' + rowdata.FrozenID + ' data-companyname=' + rowdata.CompanyName + ' data-providername=' + rowdata.ProviderName + ' data-companyfrozenamount=' + rowdata.CompanyFrozenAmount + ' data-paymentserial=' + rowdata.forPaymentSerial +' onclick="ThawPoint(this)" >解冻</button>';
                        else
                            retValue = '';
                        return retValue;
                    }
                }
            ],
            rowCallback: function (row, data) { },
            filter: true,         //右上搜尋
            info: false,
            ordering: true, //排序
            processing: true,
            retrieve: true,
            paging: true,    //分頁
            scrollX: true,
            autoWidth: false,
            language: chineseTableLang
        });
    }

    $('#btnSearch').click(function () {
        getProxyProviderGroup('LPay2');
        getTableResult();
    })

    function addImage(obj) {
        var id = $(obj).data('frozenid');
        $(obj).parent().append('<br/><input  data-type="create" onchange="pictureChange(this)" data-frozenid="' + id + '" type="file" style="border-radius:0px;border:0px;"  accept="image/gif, image/jpeg, image/png" /><div><span class="span_CreateDate" style="margin-right:5px"></span><button style="margin-top:5px;" src= "">未上传</button></div>');
    }

    function deleteImage(doc) {
        var ImageID = $(doc).data('imageid');

        swal({
            title: "是否要删除图片?",
            type: "warning",
            showCancelButton: true,
            closeOnConfirm: true,
            confirmButtonText: "确定",
            cancelButtonText: "取消",
            confirmButtonColor: "#DD6B55",
        },
            function (isConfirm) {
                if (isConfirm) {

                    if (!ImageID) {
                        parent.successAlert(function () {
                            getTableResult();
                        });
                    } else {
                        api.deleteImage(ImageID, function (success, obj) {
                            parent.wrapperFadeIn();
                            if (success) {
                                parent.wrapperFadeOut();
                                if (obj.ResultCode == 0) {
                                    parent.successAlert(function () {
                                        getTableResult();
                                    });
                                } else {
                                    var message = ""
                                    switch (obj.ResultCode) {
                                        case 6:
                                            message = "删除失败";
                                            parent.warningAlert(message);
                                            break;
                                        case 7:
                                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                            break;
                                        default:
                                            message = "其他错误";
                                            parent.warningAlert(message);
                                            break;
                                    }
                                }

                            } else {
                                parent.wrapperFadeOut();
                                parent.warningAlert("系统有误");
                            }

                        });
                    }
                }
            });
    }

    function getProviderCodeResult() {
        api.getProviderCodeResultByShowType(function (success, obj) {
            if (obj.ResultCode == 0) {
                createProviderSelect(obj.ProviderTypes);
                parent.wrapperFadeOut();
                //getTableResult();
            } else {
                var message = ""
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                    case 5:

                        break;
                    default:
                        message = "其他错误";
                        parent.warningAlert(message);
                        break;
                }
                parent.wrapperFadeOut();

            }
        });
    }

    function createProviderSelect(providerdata) {

        if (providerdata) {
            $("#select_providercode").append('<option value="-99">全部</option>');
            $.each(providerdata, function (i, item) {
                $("#select_providercode").append('<option value="' + providerdata[i].ProviderCode + '">' + providerdata[i].ProviderName + '</option>');
            })

            $('#select_providercode').selectpicker('refresh');
        }
    }

    function CreateCompanySelect() {
        api.getCompanyTableResult(function (success, obj) {
            if (obj.ResultCode == 0) {
                if (obj.CompanyResults.length > 0) {
                    $("#select_Company").children().remove();
                    $("#select_Company").append('<option value="0">全部</option>');
                    $.each(obj.CompanyResults, function (i, item) {
                        $("#select_Company").append('<option value="' + obj.CompanyResults[i].CompanyID + '">' + obj.CompanyResults[i].CompanyName + '</option>');
                    })

                    $('#select_Company').selectpicker('refresh');
                }
                parent.wrapperFadeOut();
            } else {
                var message = ""
                switch (obj.ResultCode) {
                    case 7:
                        parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                        break;
                }
                parent.wrapperFadeOut();
            }
        });
    }

    function createTypeSelect() {
        //0=代收/1=代付/2=提現/3=後台加扣
        $("#select_Type").append('<option value="99">' + '所有' + '</option>');
        $("#select_Type").append('<option value="0">' + '冻结' + '</option>');
        $("#select_Type").append('<option value="1">' + '解冻' + '</option>');
        $('#select_Type').selectpicker('refresh');
    }
    
    function ThawPoint(e) {

        var forPaymentSerial = $(e).data("paymentserial");
        var CompanyFrozenAmount = $(e).data("companyfrozenamount");
        var ProviderName = $(e).data("providername");
        var CompanyName = $(e).data("companyname");
        var FrozenID = $(e).data("frozenid");
        swal({
            title: "案件解冻",
            text: `冻结单号:${forPaymentSerial}<br/>商户:${CompanyName}<br/>渠道:${ProviderName}<br/>商户冻结金额:${CompanyFrozenAmount}`,
            type: "warning",
            html: true,
            showCancelButton: true,
            closeOnConfirm: true,
            confirmButtonText: "确定",
            cancelButtonText: "取消",
            confirmButtonColor: "#DD6B55",
        },
            function (isConfirm) {
                if (isConfirm) {
                    api.ThawPoint(FrozenID, function (success, obj) {
                        if (obj.ResultCode == 0) {
                            getProxyProviderGroup('LPay2');
                            parent.warningAlert("解冻成功");
                            getTableResult();
                        } else {
                            var message = ""
                            parent.warningAlert("解冻失败！失败代码:" + obj.ResultCode);
                            getTableResult();
                        }
                    });
                }
            });
        //swal({
        //    title: "代付渠道供应商",
        //    text: strswaltext,
        //    html: true,
        //    confirmButtonText: "确认"
        //});

    }
</script>
