<body class="theme-cyan">
    <div class="block-header paper-wrap">
        <div class="row">
            <div class="col col-lg-3 col-md-3 col-sm-3">
                <h2>商戶渠道金額調整</h2>
            </div>
            <div class="col col-lg-9 col-md-9 col-sm-9">
                <div class="tips">
                    <span></span>
                </div>
            </div>

        </div>
    </div>

    <div class="container-fluid">
        <!-- Vertical Layout -->
        <div class="row clearfix">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="card">
                    <div class="header"></div>
                    <div class="body">
                        <form>
                            <label>供应商 :</label>
                            <select class="form-control show-tick" id="select_providercode"></select>
                            <label>商户 :</label>
                            <select class="form-control show-tick" id="select_company"></select>
                            <label>支付类型 :</label>
                            <select class="form-control show-tick" id="select_servicetype">
                                <option>请先选择一间商户</option>
                            </select>
                            <label>币别 :</label>
                            <select class="form-control show-tick" id="select_currency"><option>请先选择一间商户</option></select>
                            <label>类型 :</label>
                            <div class="form-group">
                                <div class="radio inlineblock">
                                    <input type="radio" name="Type" id="Type0" class="with-gap" value="0" checked="checked">
                                    <label for="Type0">代收</label>
                                </div>
                                <div class="radio inlineblock">
                                    <input type="radio" name="Type" id="Type1" class="with-gap" value="1">
                                    <label for="Type1">代付</label>
                                </div>
                            </div>
                            <label for="Amount">金额 : </label>
                            <div class="form-group">
                                <input type="number" id="Amount" class="form-control thousand-symbols">
                            </div>
                            <label for="Description">备注 : </label>
                            <div class="form-group">
                                <input type="text" id="Description" class="form-control thousand-symbols">
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-raised btn-primary btn-round waves-effect" id="btn_Save">储存</button>
                                <button type="button" class="btn btn-raised btn-primary btn-round waves-effect" id="btn_Cancel">取消</button>
