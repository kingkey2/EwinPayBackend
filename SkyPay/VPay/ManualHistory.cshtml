                        </form>
                    </div>
                    <div class="footer"></div>
                </div>
            </div>
        </div>
        <!-- #END# Vertical Layout -->
    </div>
</body>
    <script>

        $(document).ready(function () {
            pageLoad();
        });

        function pageLoad() {
            
if ('@CheckLoginPermission' == 'false') {
                location.href = "Login.cshtml";
            }

            AdminObject = parent.AdminObject;
            api = parent.api;
            //建立下拉
            CreateCompanySelect();
            getProviderCodeResult();
            parent.wrapperFadeOut();
        }


        function getProviderCodeResult() {
            api.getProviderCodeResultByShowType(function (success, obj) {
                if (obj.ResultCode == 0) {
                    createProviderSelect(obj.ProviderTypes);
                    parent.wrapperFadeOut();
                } else {
                    var message = ""
                    switch (obj.ResultCode) {
                        case 7:
                            parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                            break;
                        case 5:

                            break;
                        default:
                            message = "其他错误";
                            parent.warningAlert(message);
                            break;
                    }
                    parent.wrapperFadeOut();

                }
            });
        }

        function createProviderSelect(providerdata) {

            if (providerdata) {
                $("#select_providercode").append('<option value="-99">选择供应商</option>');
                $.each(providerdata, function (i, item) {
                    $("#select_providercode").append('<option value="' + providerdata[i].ProviderCode + '">' + providerdata[i].ProviderName + '</option>');
                })

                $('#select_providercode').selectpicker('refresh');
            }
        }
        //儲存
        $("#btn_Save").on("click", function () {
            var TransactionSerial = '';
            var Amount = $("#Amount").val().trim();
            var Description = $("#Description").val().trim();
            var Type = $('input[name=Type]:checked').val();
            var company = $('#select_company').val();
            var servicetype = $('#select_servicetype').val();
            var currency = $("#select_currency").val();
            var providerCode = $("#select_providercode").val();
            //#region 驗證
            if (providerCode == -99) {
                parent.warningAlert("请先选择供应商");
                return;
            }

            if (company == -99 || servicetype == -99) {
                parent.warningAlert("选择正确商户或支付类型");
                return;
            }

            if ($("#Amount").val().trim() == "") {
                parent.warningAlert("请先输入金额");
                return;
            }

            if ($("#Description").val().trim() == "") {
                parent.warningAlert("请先输入备注");
                return;
            }
            //#endregion
            parent.wrapperFadeIn();
            api.insertManualHistory(company, servicetype, TransactionSerial, Amount, Description, Type, currency, providerCode, function (success, obj) {
                if (success) {
                    parent.wrapperFadeOut();
                    if (obj.ResultCode == 0) {
                        parent.successAlert(function () {
                            parent.CloseActivePage(window.frameElement.name);
                        });
                    } else {
                        var message = ""
                        switch (obj.ResultCode) {
                            case 4:
                                parent.errorAlertCallback("权限不足", function () {
                                    parent.ChangePage("Home");
                                    parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                });
                                break;
                            case 6:
                                message = obj.Message;
                                parent.warningAlert(message);
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            case 8:
                                message = "服务类型重复";
                                break;
                            default:
                                message = "其他错误";
                                break;
                        }

                        parent.warningAlert(message);
                    }
                }
            });
        })
        //取消回檢視頁面
        $("#btn_Cancel").on("click", function () {
            parent.CloseActivePage(window.frameElement.name);

        })

        function CreateCompanySelect() {
            $('#select_company').html("");
            let strDetail = "";
            api.getCompanyTableResult(function (s, o) {
                if (s) {
                    if (o.ResultCode == 0) {
                        $("#select_company").append('<option value="-99">选择商户</option>');
                        $.each(o.CompanyResults, function (i, item) {
                            $("#select_company").append('<option value="' + o.CompanyResults[i].CompanyID + '">' + o.CompanyResults[i].CompanyName + '</option>');
                        })

                        $('#select_company').change(function () {
                            parent.wrapperFadeIn();
                            CreateServiceTypeSelect($(this).val());
                            createCurrencySelect($(this).val());
                        });
                        $('#select_company').selectpicker('refresh');

                    } else {
                        switch (o.ResultCode) {
                            case 4:
                                parent.errorAlertCallback("权限不足", function () {
                                    parent.ChangePage("Home");
                                    parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                });
                                break;
                            case 5:
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            default:
                                parent.warningAlert("其他错误");
                                break;
                        }
                    }
                    //消除遮罩
                    parent.wrapperFadeOut();
                }
            });

        }

        function CreateServiceTypeSelect(companyid) {
            $('#select_servicetype').html("");
            let strCompanyPointDetail = "";
            api.getCompanyServiceTableResult(companyid, function (s, o) {
                if (s) {
                    if (o.ResultCode == 0) {
                        $("#select_servicetype").append('<option value="-99">请选择支付类型</option>');
                        $.each(o.CompanyServiceResults, function (i, item) {
                            $("#select_servicetype").append('<option value="' + o.CompanyServiceResults[i].ServiceType + '">' + o.CompanyServiceResults[i].ServiceTypeName + '</option>');
                        })
                        $('#select_servicetype').selectpicker('refresh');

                    } else {
                        switch (o.ResultCode) {
                            case 4:
                                parent.errorAlertCallback("权限不足", function () {
                                    parent.ChangePage("Home");
                                    parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                });
                                break;
                            case 5:
                                $("#select_servicetype").append('<option value="-99">该商户未设定支付类型</option>');
                                $('#select_servicetype').selectpicker('refresh');
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            default:
                                $("#select_servicetype").append('<option value="-99">该商户未设定支付类型</option>');
                                $('#select_servicetype').selectpicker('refresh');
                                break;
                        }
                    }
                    //消除遮罩
                    parent.wrapperFadeOut();
                }
            });
        }

        function createCurrencySelect(selCompanyid) {

            $('#select_currency').html("");
            let strCompanyPointDetail = "";
            api.getCompanyPointTableResult(selCompanyid, function (s, o) {
                if (s) {
                    if (o.ResultCode == 0) {
                        $.each(o.CompanyPoints, function (i, item) {
                            $("#select_currency").append('<option value="' + o.CompanyPoints[i].CurrencyType + '">' + o.CompanyPoints[i].CurrencyType + '</option>');
                        })
                        $('#select_currency').selectpicker('refresh');

                    } else {
                        switch (o.ResultCode) {
                            case 4:
                                parent.errorAlertCallback("权限不足", function () {
                                    parent.ChangePage("Home");
                                    parent.CloseActivePage(window.frameElement.id.split('_')[1]);
                                });
                                break;
                            case 5:
                                break;
                            case 7:
                                parent.errorAlert("您已断线请重新登入", "Login.cshtml");
                                break;
                            default:
                                $("#select_currency").append('<option value="-99">该商户未设定币别</option>');
                                $('#select_currency').selectpicker('refresh');
                                break;
                        }
                    }
                    //消除遮罩
                    parent.wrapperFadeOut();
                }
            });
        }
    </script>
